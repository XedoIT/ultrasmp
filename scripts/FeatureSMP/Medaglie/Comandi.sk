                            send "{@error_prefix} &6%player% &cha cercato di usare il comando &e/medals %arg-1%&c." to console
                            stop

                    else if arg-3 is "fishing":
                        if player has permission "medals.grant.fishing":
                            if {medals.%arg-2%.fishing} is "{@not_get}":
                                set {medals.%player%.fishing.cod} to true
                                set {medals.%player%.fishing.salmon} to true
