  # SUONI  
    confirm_sound: block.note_block.pling
    error_sound: entity.enderman.teleport
    medals_sound: entity.player.levelup
  
  # FRASI STATO MEDAGLIE
    get: &aQuesta medaglia l'hai ottenuta.
    not_get: &cQuesta medaglia non l'hai ancora ottenuta.

  # INDICATORI DI COMPLETAMENTO MEDAGLIA "TAMING"
    done: &a✓
    not_done: &c❌

command /medals [<text>] [<offline player>] [<text>]:
    aliases: /medaglie
    description: Visualizza e gestisci le medaglie di un giocatore.
    executable by: player, console
    trigger:
        if sender is player:
            if arg-1 is not set:
                if arg-2 is not set:
                    if arg-3 is not set:
                        openMedalsGui(player)
                        stop
            else if arg-1 is "grant":
                if arg-2 is not set:
                    if arg-3 is not set:
                        if player has permission "medals.grant.use":
                            send "{@prefix} &cComando incompleto."
                            send "{@prefix} &cUtilizzo: &6/medals grant <&egiocatore&6> <&emedaglia&6>"
                            stop
                        else:
                            send "{@prefix_hide} &cComando sconosciuto."
                            send "{@error_prefix} &6%player% &cha cercato di usare il comando &e/medals %arg-1%&c." to console
                            stop
                else if arg-2 is online:
                    if arg-3 is not set:
                        if player has permission "medals.grant.use":
                            send "{@prefix} &cComando incompleto."
                            send "{@prefix} &cUtilizzo: &6/medals grant <&egiocatore&6> <&emedaglia&6>"
                            stop
                        else:
                            send "{@prefix_hide} &cComando sconosciuto."
                            send "{@error_prefix} &6%player% &cha cercato di usare il comando &e/medals %arg-1%&c." to console
                            stop

                    else if arg-3 is "mining":
                        if player has permission "medals.grant.mining":
                            if {medals.%arg-2%.mining} is "{@not_get}":
                                set {_pl} to "%arg-2%" parsed as player
                                set {medals.%arg-2%.mining} to "{@get}"
                                set {medals.%arg-2%.mining.when_get} to now
                                set {medals.%arg-2%.mining.count} to 50000
                                set {medals.%arg-2%.last_obtained} to "&aEstrazione"
                                add 1 to {medals.%arg-2%}
                                add 350 to {points.%arg-2%}
                                set {medals.%arg-2%.mining.can_use_pickaxe} to true
                                give 1 {@mining_pickaxe} to {_pl}
                                play sound "{@medals_sound}" with volume 5 and pitch 2 at arg-2 for arg-2
                                send "&6&m----------------------------------------" to arg-2
                                send " &b&k0 &eMedaglia ottenuta! &b&k0  &a&lEstrazione" to arg-2
                                send "&f" to arg-2
                                send " &9Ricompense sbloccate:" to arg-2
                                send "  &b+350 punti" to arg-2
                                send "  &aHai sbloccato il &5MineKinetor" to arg-2
                                send "&6&m----------------------------------------" to arg-2
                                send "{@prefix} &6%player% &7ti ha dato la medaglia &eEstrazione&7." to arg-2
                                send "{@prefix} &aHai assegnato la medaglia &eEstrazione &aa &b%arg-2%&e" to player
                                play sound "{@confirm_sound}" with volume 1 and pitch 2 at player for player
                                send "{@prefix} &6%player% &aha assegnato la medaglia &eEstrazione &aa &b%arg-2%&a." to console
                                stop
                            else:
                                send "{@prefix} &6%arg-2% &cha già ottenuto la medaglia &eEstrazione&c."
                                play sound "{@error_sound}" with volume 1 and pitch 0 at player for player
                                stop
                        else:
                            send "{@prefix_hide} &cComando sconosciuto."
                            send "{@error_prefix} &6%player% &cha cercato di usare il comando &e/medals %arg-1%&c." to console
                            stop

                    else if arg-3 is "mobkiller":
                        if player has permission "medals.grant.mobkiller":
                            if {medals.%arg-2%.mobkiller} is "{@not_get}":
                                set {medals.%arg-2%.mobkiller} to "{@get}"
                                set {medals.%arg-2%.mobkiller.when_get} to now
                                set {medals.%arg-2%.mobkiller.count} to 5000
                                set {medals.%arg-2%.last_obtained} to "&aMobicida"
                                add 1 to {medals.%arg-2%}
                                add 200 to {points.%arg-2%}
                                apply strength 5 without any particles to arg-2 for 2 minutes
                                play sound "{@medals_sound}" with volume 5 and pitch 2 at arg-2 for arg-2
                                send "&6&m----------------------------------------" to arg-2
                                send " &b&k0 &eMedaglia ottenuta! &b&k0  &a&lMobicida" to arg-2
                                send "&f" to arg-2
                                send " &9Ricompense sbloccate:" to arg-2
                                send "  &a+200 punti" to arg-2
                                send "  &b+2 minuti di &4Forza V" to arg-2
                                send "&6&m----------------------------------------" to arg-2
                                send "{@prefix} &6%player% &7ti ha dato la medaglia &eMobicida&7." to arg-2
                                send "{@prefix} &aHai assegnato la medaglia &eMobicida &aa &b%arg-2%&e" to player
                                play sound "{@confirm_sound}" with volume 1 and pitch 2 at player for player
                                send "{@prefix} &6%player% &aha assegnato la medaglia &eMobicida &aa &b%arg-2%&a." to console
                                stop
                            else:
                                send "{@prefix} &6%arg-2% &cha già ottenuto la medaglia &eMobicida&c."
                                play sound "{@error_sound}" with volume 1 and pitch 0 at player for player
                                stop
                        else:
                            send "{@prefix_hide} &cComando sconosciuto."
                            send "{@error_prefix} &6%player% &cha cercato di usare il comando &e/medals %arg-1%&c." to console
