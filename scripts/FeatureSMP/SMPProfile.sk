    status_prefix: &4[&e&lSTATUS&4]

    view: &eClicca per visualizzare altro.
    more_info: &eClicca per controllare i dettagli.
    no_gui: &cQuesta pagina non √® ancora disponibile!
    glinted: {HideFlags:1,Enchantments:[{id:""minecraft:unbreaking"",lvl:1s}]}
    emoji_template: {display:{Name:""{\""text\"":\""Emoticon Smile\""}""},SkullOwner:{Id:[I;2002063258,-175029939,-2024958054,1439521797],Properties:{textures:[{Value:""eyJ0ZXh0dXJlcyI6eyJTS0lOIjp7InVybCI6Imh0dHA6Ly90ZXh0dXJlcy5taW5lY3JhZnQubmV0L3RleHR1cmUvZmQ2ZDVmZDUxMmEzNDdjOGM4Y2JhYjRjYzlhZjNlNTMyNDlkOGQyMWQ1N2Y0YWE1ZTIxZGE5MzgxZGFlNmYifX19""}]}}}
    no_flags: {HideFlags:255}

    male: &9Maschio
    female: &dFemmina

    status: &cNessuno stato impostato.
    info_wiki: &eClicca per aprire la GUI della Wiki.

on join:
    if {status.%player%} is not set:
        set {status.%player%} to "{@status}"
        
command /profilo:
    aliases: /smp, /profile
    description: Apri e gestisci il tuo profilo SMP.
    executable by: player, console
    trigger:
        if sender is player:
            openProfileGui(player)
            stop
        else:
            send "&cSolo i giocatori possono aprire il proprio profilo!"
            stop

command /status [<text>] [<text>]:
    aliases: /stato
    permission: status.use
    permission message: &4[&6&lS&e&lM&6&lP&4] &cComando sconosciuto.
    description: Imposta uno stato personale
    executable by: player, console
    trigger:
        if sender is player:
            if arg-1 is not set:
                if {status.%player%} is not "{@status}":
                    send "{@status_prefix} &fIl tuo stato attuale: &7%{status.%player%}%"
                    stop
                else:
                    send "{@status_prefix} &cNon hai ancora uno stato."
                    send "{@status_prefix} &cImpostalo con &6/status set <stato>&c."
                    play sound "entity.enderman.teleport" with volume 1 and pitch 0 at player for player
                    stop
            else if arg-1 is "set":
                if arg-2 is not set:
                    send "{@status_prefix} &cComando incompleto."
                    send "{@status_prefix} &cUtilizzo: &6/status set <stato>"
                    play sound "entity.enderman.teleport" with volume 1 and pitch 0 at player for player
                    stop
                else:
                    if {status.%player%} is not "{@status}":
                        send "{@status_prefix} &aHai aggiornato il tuo stato."
                        set {status.%player%.old} to {status.%player%}
                        delete {status.%player%}
                    else:
                        send "{@status_prefix} &aHai impostato il tuo stato."
                    set {status.%player%} to arg-2
                    send "{@status_prefix} &7%{status.%player%}%"
                    play sound "block.note_block.pling" with volume 1 and pitch 2 at player for player
                    stop
            else if arg-1 is "delete":
                if arg-2 is not set:
                    if {status.%player%} is not "{@status}":
                        if {settings.%player%.user.genre} is "{@male}":
                            send formatted "{@status_prefix} &cSei sicuro di voler eliminare il tuo stato? <suggest command:s√¨>&2&l[S√å]<reset> <suggest command:no>&4&l[NO]<reset>"
                            play sound "entity.player.levelup" with volume 1 and pitch 2 at player for player
                            set {status.%player%.confirm} to true
                            stop
                        if {settings.%player%.user.genre} is "{@female}":
                            send formatted "{@status_prefix} &cSei sicura di voler eliminare il tuo stato? <suggest command:s√¨>&2&l[S√å]<reset> <suggest command:no>&4&l[NO]<reset>"
                            play sound "entity.player.levelup" with volume 1 and pitch 2 at player for player
                            set {status.%player%.confirm} to true
                            stop
                    else:
                        send "{@status_prefix} &cNon puoi eliminare il tuo stato se prima non ne imposti uno!"
                        play sound "entity.enderman.teleport" with volume 1 and pitch 0 at player for player
                        stop
                else:
                    send "{@status_prefix} &cQuesto comando non necessita di ulteriori parametri."
                    stop
            else if arg-1 is not "set" or "delete":
                send "{@status_prefix} &cComando sconosciuto."
                send "{@status_prefix} &cUtilizzo: &6/status set <stato>"
                send "{@status_prefix} &cUtilizzo: &6/status delete"
                stop
        else:
            send "{@status_prefix} &cSolo i giocatori possono eseguire questo comando."
            stop

on tab complete of "/status" or "stato":
    set tab completions for position 1 to "set" and "delete"
    if tab arg-1 is "set":
        set tab completions for position 2 to "[<stato>]"
        
on chat:
    if {status.%player%.confirm} is true:
        cancel event
        if message is "si", "s√¨", "yes", "s" or "y":
            delete {status.%player%}
            set {status.%player%} to "{@status}"
            send "{@status_prefix} &aStato eliminato con successo." to player
            play sound "block.note_block.pling" with volume 1 and pitch 2 at player for player
            delete {status.%player%.confirm}
        else:
            send "{@status_prefix} &aRicevuto." to player
            play sound "block.note_block.pling" with volume 1 and pitch 2 at player for player
            delete {status.%player%.confirm}

