command /profilo:
    aliases: /smp, /profile
    description: Apri e gestisci il tuo profilo SMP.
    executable by: player, console
    trigger:
        if sender is player:
            openProfileGui(player)
            stop
        else:
            send "&cSolo i giocatori possono aprire il proprio profilo!"
            stop

