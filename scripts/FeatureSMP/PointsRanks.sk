# Script per il sistema dei punti, reputazioni ed effetti dati dalle reputazioni #
#
# PlaceHolder per i PUNTI: %skript_points.{player}%
# PlaceHolder per gli EFFETTI: %skript_points.{player}.effect%
#
# PERMESSI
#
# PUNTI
# - points.*
# - points.commands
# - points.check.self
# - points.check.other
# - points.reset.self
# - points.reset.other
# - points.set
# - points.add
# - points.remove
# - points.admin.help
# - points.help
# - points.reload
# - points.idle.notify
#
# REPUTAZIONI
# - ranks.*
# - ranks.commands
# - ranks.check.self
# - ranks.check.other
# - ranks.reset.self
# - ranks.reset.other
# - ranks.set
# - ranks.list
# - ranks.admin.help
# - ranks.help
#

# Impostazioni per Punti e Reputazioni
options:
    p_prefix: &8[&b&lPUNTI&8]
    p_error: &8[&4&lPUNTI&8]

    no_perm: &4[&6&lS&e&lM&6&lP&4]

    r_prefix: &8[&b&lRANK&8]
    r_error: &8[&4&lRANK&8]
    
    version: 0.4

    confirm_sound: block.note_block.pling
    error_sound: entity.enderman.teleport
    check_sound: entity.player.levelup
    what_sound: entity.villager.trade

on join:
    if {points.%player%} is not set:
        set {points.%player%} to 0
    if {points.%player%.rank} is not set:
        set {points.%player%.rank} to "&7&lNeutrale"
    if {points.%player%.rankprefix} is not set:
        set {points.%player%.rankprefix} to "&8[&7&lN&8]"
    if {points_idle} is not set:
        set {points_idle} to true

    if {settings.%player%.genre.male} is true:
        set {_alignment-5} to "&4&lMalvagio"
        set {_alignment-4} to "&c&lPessimo"
        set {_alignment-3} to "&6&lVile"
        set {_alignment-2} to "&e&lSpregevole"
        set {_alignment-1} to "&8&lAggressivo"
        set {_alignment0} to "&7&lNeutrale"
        set {_alignment1_1} to "&a&lGentile"
        set {_alignment1_2} to "&a&lGentile&e⁺"
        set {_alignment2_1} to "&2&lBuono"
        set {_alignment2_2} to "&2&lBuono&e⁺"
        set {_alignment3_1} to "&3&lPuro"
        set {_alignment3_2} to "&3&lPuro&e⁺"
        set {_alignment4_1} to "&9&lMigliore"
        set {_alignment4_2} to "&9&lMigliore&e⁺"
        set {_alignment5_1} to "&b&lSupremo"
        set {_alignment5_2} to "&b&lSupremo&e⁺"
    else if {settings.%player%.genre.female} is true:
        set {_alignment-5} to "&4&lMalvagia"
        set {_alignment-4} to "&c&lPessima"
        set {_alignment-3} to "&6&lVile"
        set {_alignment-2} to "&e&lSpregevole"
        set {_alignment-1} to "&8&lAggressiva"
        set {_alignment0} to "&7&lNeutrale"
        set {_alignment1_1} to "&a&lGentile"
        set {_alignment1_2} to "&a&lGentile&e⁺"
        set {_alignment2_1} to "&2&lBuona"
        set {_alignment2_2} to "&2&lBuona&e⁺"
        set {_alignment3_1} to "&3&lPura"
        set {_alignment3_2} to "&3&lPura&e⁺"
        set {_alignment4_1} to "&9&lMigliore"
        set {_alignment4_2} to "&9&lMigliore&e⁺"
        set {_alignment5_1} to "&b&lSuprema"
        set {_alignment5_2} to "&b&lSuprema&e⁺"
#--------------------------------------------#
#                                            #
#                  GENERALE                  #
#                                            #
#--------------------------------------------#
every 5 hours:
    loop all players:
        if {settings.%loop-player%.genre.male} is true:
            set {_alignment-5} to "&4&lMalvagio"
            set {_alignment-4} to "&c&lPessimo"
            set {_alignment-3} to "&6&lVile"
            set {_alignment-2} to "&e&lSpregevole"
            set {_alignment-1} to "&8&lAggressivo"
            set {_alignment0} to "&7&lNeutrale"
            set {_alignment1_1} to "&a&lGentile"
            set {_alignment1_2} to "&a&lGentile&e⁺"
            set {_alignment2_1} to "&2&lBuono"
            set {_alignment2_2} to "&2&lBuono&e⁺"
            set {_alignment3_1} to "&3&lPuro"
            set {_alignment3_2} to "&3&lPuro&e⁺"
            set {_alignment4_1} to "&9&lMigliore"
            set {_alignment4_2} to "&9&lMigliore&e⁺"
            set {_alignment5_1} to "&b&lSupremo"
            set {_alignment5_2} to "&b&lSupremo&e⁺"
        else if {settings.%loop-player%.genre.female} is true:
            set {_alignment-5} to "&4&lMalvagia"
            set {_alignment-4} to "&c&lPessima"
            set {_alignment-3} to "&6&lVile"
            set {_alignment-2} to "&e&lSpregevole"
            set {_alignment-1} to "&8&lAggressiva"
            set {_alignment0} to "&7&lNeutrale"
            set {_alignment1_1} to "&a&lGentile"
            set {_alignment1_2} to "&a&lGentile&e⁺"
            set {_alignment2_1} to "&2&lBuona"
            set {_alignment2_2} to "&2&lBuona&e⁺"
            set {_alignment3_1} to "&3&lPura"
            set {_alignment3_2} to "&3&lPura&e⁺"
            set {_alignment4_1} to "&9&lMigliore"
            set {_alignment4_2} to "&9&lMigliore&e⁺"
            set {_alignment5_1} to "&b&lSuprema"
            set {_alignment5_2} to "&b&lSuprema&e⁺"
        set {_points-5} to a random integer between 300 and 400
        set {_points-4} to a random integer between 250 and 300
        set {_points-3} to a random integer between 200 and 250
        set {_points-2} to a random integer between 100 and 200
        set {_points-1} to a random integer between 50 and 100
        set {_points0} to a random integer between 10 and 50
        set {_points1_1} to a random integer between 50 and 75
        set {_points1_2} to a random integer between 50 and 125
        set {_points2_1} to a random integer between 125 and 175
        set {_points2_2} to a random integer between 125 and 200
        set {_points3_1} to a random integer between 225 and 250
        set {_points3_2} to a random integer between 225 and 275
        set {_points4_1} to a random integer between 250 and 325
        set {_points4_2} to a random integer between 250 and 350
        set {_points5_1} to a random integer between 350 and 450
        set {_points5_2} to a random integer between 450 and 500
        if loop-player is online:
            if {points.%loop-player%} < 1000000:
                if {points_idle} is true:
                    if {points.%loop-player%.rank} is {_alignment-5}:
                        add {_points-5} to {points.%loop-player%}
                        send "&6+%{_points-5}% punti! (Tempo di gioco) &b&l>> &6(%{_alignment-5}%&6)" to loop-player
                        stop
                    else if {points.%loop-player%.rank} is {_alignment-4}:
                        add {_points-4} to {points.%loop-player%}
                        send "&6+%{_points-4}% punti! (Tempo di gioco) &b&l>> &6(%{_alignment-4}%&6)" to loop-player
                        stop
                    else if {points.%loop-player%.rank} is {_alignment-3}:
                        add {_points-3} to {points.%loop-player%}
                        send "&6+%{_points-3}% punti! (Tempo di gioco) &b&l>> &6(%{_alignment-3}%&6)" to loop-player
                        stop
                    else if {points.%loop-player%.rank} is {_alignment-2}:
                        add {_points-2} to {points.%loop-player%}
                        send "&6+%{_points-2}% punti! (Tempo di gioco) &b&l>> &6(%{_alignment-2}%&6)" to loop-player
                        stop
                    else if {points.%loop-player%.rank} is {_alignment-1}:
                        add {_points-1} to {points.%loop-player%}
                        send "&6+%{_points-1}% punti! (Tempo di gioco) &b&l>> &6(%{_alignment-1}%&6)" to loop-player
                        stop
                    else if {points.%loop-player%.rank} is {_alignment0}:
                        add {_points0} to {points.%loop-player%}
                        send "&6+%{_points0}% punti! (Tempo di gioco) &b&l>> &6(%{_alignment0}%&6)" to loop-player
                        stop
                    else if {points.%loop-player%.rank} is {_alignment1_1}:
                        add {_points1_1} to {points.%loop-player%}
                        send "&6+%{_points1_1}% punti! (Tempo di gioco) &b&l>> &6(%{_alignment1_1}%&6)" to loop-player
                        stop
                    else if {points.%loop-player%.rank} is {_alignment1_2}:
                        add {_points1_2} to {points.%loop-player%}
                        send "&6+%{_points1_2}% punti! (Tempo di gioco) &b&l>> &6(%{_alignment1_2}%&6)" to loop-player
                        stop
                    else if {points.%loop-player%.rank} is {_alignment2_1}:
                        add {_points2_1} to {points.%loop-player%}
                        send "&6+%{_points2_1}% punti! (Tempo di gioco) &b&l>> &6(%{_alignment2_1}%&6)" to loop-player
                        stop
                    else if {points.%loop-player%.rank} is {_alignment2_2}:
                        add {_points2_2} to {points.%loop-player%}
                        send "&6+%{_points2_2}% punti! (Tempo di gioco) &b&l>> &6(%{_alignment2_2}%&6)" to loop-player
                        stop
                    else if {points.%loop-player%.rank} is {_alignment3_1}:
                        add {_points3_1} to {points.%loop-player%}
                        send "&6+%{_points3_1}% punti! (Tempo di gioco) &b&l>> &6(%{_alignment3_1}%&6)" to loop-player
                        stop
                    else if {points.%loop-player%.rank} is {_alignment3_2}:
                        add {_points3_2} to {points.%loop-player%}
                        send "&6+%{_points3_2}% punti! (Tempo di gioco) &b&l>> &6(%{_alignment3_2}%&6)" to loop-player
                        stop
                    else if {points.%loop-player%.rank} is {_alignment4_1}:
                        add {_points4_1} to {points.%loop-player%}
                        send "&6+%{_points4_1}% punti! (Tempo di gioco) &b&l>> %{_alignment4_1}%&6)" to loop-player
                        stop
                    else if {points.%loop-player%.rank} is {_alignment4_2}:
                        add {_points4_2} to {points.%loop-player%}
                        send "&6+%{_points4_2}% punti! (Tempo di gioco) &b&l>> (%{_alignment4_2}%&6)" to loop-player
                        stop
                    else if {points.%loop-player%.rank} is {_alignment5_1}:
                        add {_points5_1} to {points.%loop-player%}
                        send "&6+%{_points5_1}% punti! (Tempo di gioco) &b&l>> (%{_alignment5_1}%&6)" to loop-player
                        stop
                    else if {points.%loop-player%.rank} is {_alignment5_2}:
                        add {_points5_2} to {points.%loop-player%}
                        send "&6+%{_points5_2}% punti! (Tempo di gioco) &b&l>> (%{_alignment5_2}%&6)" to loop-player
                        stop
                else:
                    if loop-player has permission "points.idle.notify":
                        send "{@p_error} &cNon posso assegnare i punti ai giocatori perché l'assegnazione automatica dei punti è disattivata!" to loop-player
                        send "{@p_error} &cPer attivarla, clicca <cmd:/points idle>&6qui&c." to loop-player
                        stop
            else:
                send "{@p_prefix} &6Hai raggiunto la reputazione più alta del server, pertanto non riceverai più punti in modo automatico." to loop-player
                send "{@p_prefix} &bPuoi comunque guadagnarli giocando normalmente nel server!" to loop-player
                stop
        
command /points [<text>] [<offline player>] [<number>]:
    aliases: /punti, /p
    description: Visualizza quanti punti hai accumulato.
    executable by: player, console
    trigger:
        if sender is player:
            if arg-1 is not set:
                if arg-2 is not set:
                    if arg-3 is not set:
                        if player has permission "points.check.self":
                            if {points.%player%} is 1:
                                send "{@p_prefix} &6Hai accumulato &c%{points.%player%}% punto&6."
                                stop
                            else if {points.%player%} is -1:
                                send "{@p_prefix} &6Hai accumulato &c%{points.%player%}% punto&6."
                                stop
                            else if {points.%player%} is 0:
                                send "{@p_prefix} &6Hai &c%{points.%player%}% punti&6."
                                stop
                            else if {points.%player%} is not "-1", "0" or "1":
                                send "{@p_prefix} &6Hai accumulato &c%{points.%player%}% punti&6."
                                stop
                        else:
                            send "{@no_perm} &cComando sconosciuto."
                            stop
                    
            else if arg-1 is "check":
                if arg-2 is not set:
                    if arg-3 is not set:
                        if player has permission "points.check.other":
                            send "{@p_prefix} &cComando incompleto."
                            send "{@p_prefix} &cUtilizzo: &6/points check <&egiocatore&6>"
                            stop
                        else:
                            send "{@no_perm} &cComando sconosciuto."
                            stop
                else:
                    if arg-3 is not set:
                        if player has permission "points.check.other":
                            if {points.%arg-2%} is set:
                                send "{@p_prefix} &c%arg-2% &6ha accumulato &e%{points.%arg-2%}% punti&6."
                                stop
                            else:
                                send "{@p_error} &6%arg-2% &cnon ha ancora giocato in questo server!"
                                play sound "{@error_sound}" with volume 1 and pitch 0 at player for player
                                stop
                        else:
                            send "{@no_perm} &cComando sconosciuto."
                            stop
                    else:
                        if player has permission "points.check.other":
                            send "{@p_error} &cQuesto comando non richiede parametri aggiuntivi."
                            stop
                        else:
                            send "{@no_perm} &cComando sconosciuto."
                            stop
                    
            else if arg-1 is "reset":
                if arg-2 is not set:
                    if arg-3 is not set:
                        if player has permission "points.reset.self":
                            if {points.%player%} is not 0:
                                set {points.%player%} to 0 
                                send "{@p_prefix} &6Hai azzerato i tuoi punti."
                                play sound "{@confirm_sound}" with volume 1 and pitch 2 at player for player
                                stop
                            else:
                                send "{@p_error} &cNon hai nessun punto!"
                                play sound "{@error_sound}" with volume 1 and pitch 0 at player for player
                                stop 
                        else:
                            send "{@no_perm} &cComando sconosciuto."
                            stop
                else:
                    if arg-3 is not set:
                        if player has permission "points.reset.other":
                            if {points.%arg-2%} is not 0:
                                set {points.%arg-2%} to 0
                                send "{@p_prefix} &6Hai azzerato i punti di &c%arg-2%&6." to player
                                send "{@p_prefix} &c%player% &6ha azzerato i tuoi punti." to arg-2
                                play sound "{@confirm_sound}" with volume 1 and pitch 2 at player for player
                                stop
                            else:
                                send "{@p_error} &6%arg-2% &cnon ha nessun punto!"
                                play sound "{@error_sound}" with volume 1 and pitch 0 at player for player
                                stop
                        else:
                            send "{@no_perm} &cComando sconosciuto."
                            stop
                    else:
                        if player has permission "points.reset.other":
                            send "{@p_error} &cQuesto comando non richiede parametri aggiuntivi."
                            stop
                        else:
                            send "{@no_perm} &cComando sconosciuto."
                            stop
                        
            else if arg-1 is "set":
                if arg-2 is not set:
                    if arg-3 is not set:
                        if player has permission "points.set":
                            send "{@p_prefix} &cComando incompleto."
                            send "{@p_prefix} &cUtilizzo: &6/points set <&egiocatore&6> <&enumero&6>"
                            stop
                        else:
                            send "{@no_perm} &cComando sconosciuto."
                            stop
                else:
                    if arg-3 is not set:
                        if player has permission "points.set":
                            send "{@p_prefix} &cComando incompleto."
                            send "{@p_prefix} &cUtilizzo: &6/points set <&egiocatore&6> <&enumero&6>"
                            stop
                        else:
                            send "{@no_perm} &cComando sconosciuto."
                            stop
                    else:
                        if player has permission "points.set":
                            if arg-3 is set:
                                set {points.%arg-2%} to arg-3
                                send "{@p_prefix} &6Hai impostato i punti di &c%arg-2% &6a &e%arg-3%&6." to player
                                play sound "{@confirm_sound}" with volume 1 and pitch 2 at player for player
                                send "{@p_prefix} &c%player% &6ha impostato i tuoi punti a &e%arg-3%&6." to arg-2
                                stop
                            else:
                                send "{@p_error} &cIl numero deve essere positivo!"
                                play sound "{@error_sound}" with volume 1 and pitch 0 at player for player
                                stop    
                        else:
                            send "{@no_perm} &cComando sconosciuto."
                            stop
                
            else if arg-1 is "add":
                if arg-2 is not set:
                    if arg-3 is not set:
                        if player has permission "points.add":
                            send "{@p_prefix} &cComando incompleto."
                            send "{@p_prefix} &cUtilizzo: &6/points add <&egiocatore&6> <&enumero&6>"
                            stop
                        else:
                            send "{@no_perm} &cComando sconosciuto."
                            stop
                else:
                    if arg-3 is not set:
                        if player has permission "points.add":
                            send "{@p_prefix} &cComando incompleto."
                            send "{@p_prefix} &cUtilizzo: &6/points add <&egiocatore&6> <&enumero&6>"
                            stop
                        else:
                            send "{@no_perm} &cComando sconosciuto."
                            stop
                    else if arg-3 = 0:
                        if player has permission "points.add":
                            send "{@p_error} &cChe senso ha aggiungere 0 punti?"
                            play sound "{@what_sound}" with volume 1 at player for player
                            stop
                        else:
                            send "{@no_perm} &cComando sconosciuto."
                            stop
                    else if arg-3 > 0:
                        if player has permission "points.add":
                            add arg-3 to {points.%arg-2%}
                            send "{@p_prefix} &6Hai aggiunto &e%arg-3% punti &6ai punti totali di &c%arg-2%&6." to player
                            play sound "{@confirm_sound}" with volume 1 and pitch 2 at player for player
                            send "{@p_prefix} &c%player% &6ha aggiunto &e%arg-3% punti &6ai tuoi punti totali." to arg-2
                            stop
                        else:
                            send "{@no_perm} &cComando sconosciuto."
                            stop
                    else if arg-3 < 0:
                        if player has permission "points.add":
                            send "{@p_error} &cIl numero deve essere positivo!"
                            play sound "{@error_sound}" with volume 1 and pitch 0 at player for player
                            stop
                        else:
                            send "{@no_perm} &cComando sconosciuto."
                            stop
                
            else if arg-1 is "remove":
                if arg-2 is not set:
                    if arg-3 is not set:
                        if player has permission "points.remove":
                            send "{@p_prefix} &cComando incompleto."
                            send "{@p_prefix} &cUtilizzo: &6/points remove <&egiocatore&6> <&enumero&6>"
                            stop
                        else:
                            send "{@no_perm} &cComando sconosciuto."
                            stop
                else:
                    if arg-3 is not set:
                        if player has permission "points.remove":
                            send "{@p_prefix} &cComando incompleto."
                            send "{@p_prefix} &cUtilizzo: &6/points remove <&egiocatore&6> <&enumero&6>"
                            stop
                        else:
                            send "{@no_perm} &cComando sconosciuto."
                            stop
                    else if arg-3 = 0:
                        if player has permission "points.remove":
                            send "{@p_error} &cChe senso ha togliere 0 punti?"
                            play sound "{@what_sound}" with volume 1 at player for player
                            stop
                        else:
                            send "{@no_perm} &cComando sconosciuto."
                            stop
                    else if arg-3 > 0:
                        if player has permission "points.remove":
                            subtract arg-3 from {points.%arg-2%}
                            send "{@p_prefix} &6Hai rimosso &e%arg-3% punti &6dai punti totali di &c%arg-2%&6." to player
                            play sound "{@confirm_sound}" with volume 1 and pitch 2 at player for player
                            send "{@p_prefix} &c%player% &6ha rimosso &e%arg-3% punti &6dai tuoi punti totali." to arg-2
                            stop
                        else:
                            send "{@no_perm} &cComando sconosciuto."
                            stop
                    else if arg-3 < 0:
                        if player has permission "points.remove":
                            send "{@p_error} &cIl numero deve essere positivo!"
                            play sound "{@error_sound}" with volume 1 and pitch 0 at player for player
                            stop
                        else:
                            send "{@no_perm} &cComando sconosciuto."
                            stop
                    
            else if arg-1 is "idle":
                if arg-2 is not set:
                    if arg-3 is not set:
                        if player has permission "points.idle.toggle":
                            if {points_idle} is not set:
                                set {points_idle} to true
                                send "{@p_prefix} &7L'assegnazione automatica dei punti è stata &aattivata&7."
                                play sound "{@confirm_sound}" with volume 1 and pitch 2 at player for player
                                stop
                            else:
                                delete {points_idle}
                                send "{@p_prefix} &7L'assegnazione automatica dei punti è stata &cdisattivata&7."
                                play sound "{@confirm_sound}" with volume 1 and pitch 2 at player for player
                                stop
                        else:
                            send "{@no_perm} &cComando sconosciuto."
                            stop
                else:
                    if player has permission "points.idle.toggle":
                        send "{@p_error} &cQuesto comando non richiede parametri aggiuntivi."
                        stop
                    else:
                        send "{@no_perm} &cComando sconosciuto."
                        stop
            else if arg-1 is "help":
                if arg-2 is not set:
                    if arg-3 is not set:
                        if player has permission "points.admin.help":
                            send "&e> &7&m------------&r {@p_prefix} &7&m------------&r &e<"
                            send ""
                            send "&7Comandi che puoi eseguire:"
                            send "&c- &a/points &7&o- Visualizza quanti punti hai accumulato."
                            send "&c- &7/points &acheck <giocatore> &7&o- Controlla quanti punti ha un giocatore."
                            send "&c- &7/points &areset <giocatore> &7&o- Azzera i tuoi punti o quelli di un giocatore."
                            send "&c- &7/points &aset <giocatore> <punti> &7&o- Imposta il valore dei punti di un giocatore."
                            send "&c- &7/points &aadd <giocatore> <punti> &7&o- Aggiungi dei punti ad un giocatore."
                            send "&c- &7/points &aremove <giocatore> <punti> &7&o- Rimuovi dei punti da un giocatore."
                            send "&c- &7/points &aidle &7&o- Attiva o disattiva l'assegnazione automatica dei punti ai giocatori."
                            send "&c- &7/points &ainfo &7&o- Visualizza le info dello script."
                            send "&c- &7/points &ahelp &7&o- Mostra questa lista."
                            send "&c- &7/points &areload &7&o- Ricarica lo script."
                            send ""
                            send "&e> &7&m--------------------------------&r &e<"
                            stop
                        else:
                            send "&e> &7&m------------&r {@p_prefix} &7&m------------&r &e<"
                            send ""
                            send "&7Comandi che puoi eseguire:"
                            send "&c- &a/points &7&o- Visualizza quanti punti hai accumulato"
