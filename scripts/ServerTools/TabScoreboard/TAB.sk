options:
 # I MONDI NEL SERVER
    overworld: smp
    nether: smp_nether
    end: smp_the_end
        
every tick:
    loop all players:
        #set tab score of loop-player to {_ping} for loop-player
        if loop-player's uuid is "cd1db3e8-73ee-488f-9367-73224ca93ef3":
            set loop-player's tab name to formatted "%{points::%loop-player%::rankprefix}% &b%{nick::%loop-player%}%"
        else if loop-player's uuid is "da71a28d-8da4-427d-8967-c77757f887f4":
            set loop-player's tab name to formatted "%{points::%loop-player%::rankprefix}% &c%{nick::%loop-player%}%"
        else if loop-player's uuid is "0809c6c4-6898-415c-90d5-dd6fee334fca":
            set loop-player's tab name to formatted "%{points::%loop-player%::rankprefix}% &4%{nick::%loop-player%}%"
        else if loop-player's uuid is "d6c92f76-8c3b-4a1f-ac35-74fba37a0d44":
            set loop-player's tab name to formatted "%{points::%loop-player%::rankprefix}% &8%{nick::%loop-player%}%"
        else if loop-player's uuid is "b1cdedbc-5f5a-4d13-9f75-d0c6c24bceec":
            set loop-player's tab name to formatted "%{points::%loop-player%::rankprefix}% &a%{nick::%loop-player%}%"
