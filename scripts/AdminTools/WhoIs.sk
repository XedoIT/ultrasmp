    total_medals: 15
    
command /whois [<offline player>]:
    aliases: /playerinfo
    permission: whois.use
    permission message: &4[&6&lS&e&lM&6&lP&4] &cComando sconosciuto.
    description: Visualizza tutte le informazioni necessarie di un giocatore.
    executable by: player, console
    trigger:
        if sender is player:
            if arg-1 is not set:
                send "{@prefix} &cUtilizzo: &6/whois <giocatore>"
                stop
            else if arg-1 is online:
                set {whois.%player%.arg} to arg-1
                OnlineWhoIsGUI(player,{whois.%player%.arg})
                stop
            else if arg-1 has played before:
                set {whois.%player%.arg} to arg-1
                OfflineWhoIsGUI(player,{whois.%player%.arg})
                stop
            else:
                send "{@prefix} &cNon ho trovato informazioni per &6%arg-1%&c."
                stop
        else:
            if arg-1 is not set:
                send "{@prefix} &cUtilizzo: &6/whois <giocatore>"
                stop
            else if arg-1 is online:
                send "&6====== Informazioni per &c%arg-1% &6======"
                send "&cGENERALI"
                send "&6- Nickname: &f%{nick.%arg-1%}%"
                send "&6- UUID: &e%uuid of arg-1%"
                send "&6- Salute: &f%placeholder "aureliumskills_hp" from arg-1%/%placeholder "aureliumskills_hp_max" from arg-1% &c‚ù§"
                send "&6- Saziet√†: &f%arg-1's food level%/10 &eüçñ"
                send "&6- EXP: &a%placeholder "player_current_exp" from arg-1% (&2%placeholder "player_level" from arg-1% livelli&a)"
                send "&6- Posizione: &f(%{world.%arg-1%}%&f, &9%placeholder "player_x" from arg-1% &9%placeholder "player_y" from arg-1% &9%placeholder "player_z" from arg-1%&f, &b%placeholder "player_biome_capitalized" from arg-1%&f)"
                send "&6- Tempo di gioco totale: &f%{playtime.%arg-1%.total_days}% giorni, %{playtime.%arg-1%.total_hours}% ore, %{playtime.%arg-1%.total_minutes}% minuti e %{playtime.%arg-1%.total_seconds}% secondi"
                send "&6- Tempo di gioco della sessione attuale: &f%{playtime.%arg-1%.days}% giorni, &f%{playtime.%arg-1%.hours}% ore, &f%{playtime.%arg-1%.minutes}% minuti e &f%{playtime.%arg-1%.seconds}% secondi"
                send "&6- Fama: &f%{fame::%arg-1%}%"
                send "&6- Reputazione: %{fame.%arg-1%.rank}%"
                #send "&6- AFK: %{afk.%arg-1%.status}%"
                send "&6- Medaglie: &e%{medals.%arg-1%}%&7/&e15 &6(&7Ultima medaglia ottenuta: %{medals.%arg-1%.last_obtained}%&6)"
                send "&6- Prima volta nel server: &f%{playtime.%arg-1%.first_login}%"
                send "&bTECNICHE"
                send "&3- Indirizzo IP: &e/%placeholder "player_ip" from arg-1%"
                send "&3- Ping: &e%arg-1's ping% ms" 
                send "&3- Modalit√† di gioco: &f%{gm.%arg-1%}%"
                send "&3- Modalit√† Dio: %{god.%arg-1%}%"
                send "&3- Modalit√† Volo: %{fly.%arg-1%}%"
                send "&3- OP: %placeholder "player_is_op" from arg-1%"
                send "&3- Velocit√†: &f%arg-1's flyspeed% (Volo), %arg-1's walkspeed% (Passo)"
                stop
            else if arg-1 has played before:
                send "&6====== Informazioni per &c%arg-1% &4(Modalit√† Offline) &6======"
                send "&cGENERALI"
                send "&6- Nickname: &f%{nick.%arg-1%}%"
                send "&6- UUID: &e%uuid of arg-1%"
                send "&6- Posizione: (%{world.%arg-1%}%&f, &9%{position.%arg-1%.x}% &9%{position.%arg-1%.y}% &9%{position.%arg-1%.z}%&f, &b%{position.%arg-1%.biome}%&f)"
                send "&6- Tempo di gioco totale: &f%{playtime.%arg-1%.total_days}% giorni, %{playtime.%arg-1%.total_hours}% ore, %{playtime.%arg-1%.total_minutes}% minuti e %{playtime.%arg-1%.total_seconds}% secondi"
                send "&6- Tempo di gioco dell'ultima sessione: &f%{playtime.%arg-1%.archived_days}% giorni, &f%{playtime.%arg-1%.archived_hours}% ore, &f%{playtime.%arg-1%.archived_minutes}% minuti e &f%{playtime.%arg-1%.archived_seconds}% secondi"
                send "&6- Fama: &f%{fame::%arg-1%}%"
                send "&6- Reputazione: %{fame.%arg-1%.rank}%"
                #send "&6- AFK: %{afk.%arg-1%.status}%"
                send "&6- Medaglie: &e%{medals.%arg-1%}%&7/&e15 &6(&7Ultima medaglia ottenuta: %{medals.%arg-1%.last_obtained}%&6)"
                send "&6- Prima volta nel server: &f%{playtime.%arg-1%.first_login}%"
                send "&bTECNICHE"
                send "&3- Modalit√† di gioco: &f%{gm.%arg-1%}%"
                send "&3- Modalit√† Dio: %{god.%arg-1%}%"
                send "&3- Modalit√† Volo: %{fly.%arg-1%}%"
                send "&3- OP: %placeholder "player_is_op" from arg-1%"
                stop
            else:
                send "{@prefix} &cNon ho trovato informazioni per &6%arg-1%&c."
                stop

every second:
    loop all players:
        if world of loop-player is "smp":
            set {world.%loop-player%} to "&aOverworld"
            set {world.%loop-player%.marker} to grass block
        if world of loop-player is "smp_nether":
            set {world.%loop-player%} to "&cNether"
            set {world.%loop-player%.marker} to netherrack
        if world of loop-player is "smp_the_end":
            set {world.%loop-player%} to "&dEnd"
            set {world.%loop-player%.marker} to end stone

on join:
    delete {position.%player%.x}
    delete {position.%player%.y}
    delete {position.%player%.z}
    delete {position.%player%.biome}
    delete {offline.%player%.empty_slots}
    delete {offline.%player%.xp}
    delete {offline.%player%.xp_level}
    delete {offline.%player%.luck}
    delete {offline.%player%.health}
    delete {offline.%player%.wisdom}
    delete {offline.%player%.strength}
    delete {offline.%player%.regeneration}
    delete {offline.%player%.toughness}

on quit:
    set {position.%player%.x} to placeholder "player_x" from player
    set {position.%player%.y} to placeholder "player_y" from player
    set {position.%player%.z} to placeholder "player_z" from player
    set {position.%player%.biome} to placeholder "player_biome_capitalized" from player
    set {offline.%player%.empty_slots} to placeholder "player_empty_slots" from player
    set {offline.%player%.xp} to placeholder "player_current_exp" from player
    set {offline.%player%.xp_level} to placeholder "player_level" from player
    set {offline.%player%.luck} to placeholder "aureliumskills_luck" from player
    set {offline.%player%.health} to placeholder "aureliumskills_health" from player
    set {offline.%player%.wisdom} to placeholder "aureliumskills_wisdom" from player
    set {offline.%player%.strength} to placeholder "aureliumskills_strength" from player
    set {offline.%player%.regeneration} to placeholder "aureliumskills_regeneration" from player
    set {offline.%player%.toughness} to placeholder "aureliumskills_toughness" from player
    if world of player is "smp":
        set {world.%player%} to "&aOverworld"
        set {world.%player%.marker} to grass block
    if world of player is "smp_nether":
        set {world.%player%} to "&cNether"
        set {world.%player%.marker} to netherrack
    if world of player is "smp_the_end":
        set {world.%player%} to "&dEnd"
        set {world.%player%.marker} to end stone
    if player's tool is air:
        set {equip.%player%.mainhand} to lime stained glass pane named "&aMano principale" with lore "&6%player% &7non ha" and "&7nulla in mano."
    else:
        set {equip.%player%.mainhand} to player's held item
    if slot 40 of player's inventory is air:
        set {equip.%player%.offhand} to magenta stained glass pane named "&dMano secondaria" with lore "&6%player% &7non ha" and "&7oggetti nella seconda mano."
    else:
        set {equip.%player%.offhand} to player's off hand item
    if slot 39 of player's inventory is air:
        set {equip.%player%.helmet} to red stained glass pane named "&cSlot elmetto" with lore "&6%player% &7non sta" and "&7indossando nessun elmetto."
    else:
        set {equip.%player%.helmet} to player's helmet
    if slot 38 of player's inventory is air:
        set {equip.%player%.chestplate} to red stained glass pane named "&cSlot corazza" with lore "&6%player% &7non sta" and "&7indossando nessuna corazza."
    else:
        set {equip.%player%.chestplate} to player's chestplate
    if slot 37 of player's inventory is air:
        set {equip.%player%.leggings} to red stained glass pane named "&cSlot gambiere" with lore "&6%player% &7non sta" and "&7indossando gambiere."
    else:
        set {equip.%player%.leggings} to player's leggings
    if slot 36 of player's inventory is air:
        set {equip.%player%.boots} to red stained glass pane named "&cSlot stivali" with lore "&6%player% &7non sta" and "&7indossando stivali."
    else:
        set {equip.%player%.boots} to player's boots

