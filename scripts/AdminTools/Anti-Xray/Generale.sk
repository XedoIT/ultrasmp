# INIZIO SCRIPT (ATTENZIONE!! Modifica il codice solo se sai ciò che fai!)
options:
 # GENERALI
    prefix: &6[&e&lAnti-Xray&6]
    prefix_hide: &4[&6&lS&e&lM&6&lP&4]
    enabled: &aattivato
    disabled: &cdisattivato
    version: 1.0
    axr_directory: \AdminTools\Anti-Xray\
 # SUONI
    confirm_sound: block.note_block.pling
    error_sound: entity.enderman.teleport
    check_sound: entity.player.levelup
 # NOTIFICA MINERALI
  # FERRO
    iron_alert_msg: &7ha scavato &f%{axr_counter::%arg-1%::iron_ore}% blocchi di minerale di ferro &7in &e%{axr.timer}% secondi&7!
    deepslate_iron_alert_msg: &7ha scavato &f%{axr_counter::%arg-1%::deepslate_iron_ore}% blocchi di minerale di ferro in ardesia profonda &7in &e%{axr.timer}% secondi&7!
  # RAME
    copper_alert_msg: &7ha scavato &d%{axr_counter::%arg-1%::copper_ore}% blocchi di minerale di rame &7in &e%{axr.timer}% secondi&7!
    deepslate_copper_alert_msg: &7ha scavato &d%{axr_counter::%arg-1%::deepslate_copper_ore}% blocchi di minerale di rame in ardesia profonda &7in &e%{axr.timer}% secondi&7!
  # ORO
    gold_alert_msg: &7ha scavato &6%{axr_counter::%arg-1%::gold_ore}% blocchi di minerale d'oro &7in &e%{axr.timer}% secondi&7!
    deepslate_gold_alert_msg: &7ha scavato &6%{axr_counter::%arg-1%::deepslate_gold_ore}% blocchi di minerale d'oro in ardesia profonda &7in &e%{axr.timer}% secondi&7!
  # REDSTONE
    redstone_alert_msg: &7ha scavato &4%{axr_counter::%arg-1%::redstone_ore}% blocchi di minerale di redstone &7in &e%{axr.timer}% secondi&7!
    deepslate_redstone_alert_msg: &7ha scavato &4%{axr_counter::%arg-1%::deepslate_redstone_ore}% blocchi di minerale di redstone in ardesia profonda &7in &e%{axr.timer}% secondi&7!
  # LAPISLAZZULI
    lapis_alert_msg: &7ha scavato &9%{axr_counter::%arg-1%::lapis_ore}% blocchi di minerale di lapislazzuli &7in &e%{axr.timer}% secondi&7!
    deepslate_lapis_alert_msg: &7ha scavato &9%{axr_counter::%arg-1%::deepslate_lapis_ore}% blocchi di minerale di lapislazzuli in ardesia profonda &7in &e%{axr.timer}% secondi&7!
  # SMERALDO
    emerald_alert_msg: &7ha scavato &a%{axr_counter::%arg-1%::emerald_ore}% blocchi di minerale di smeraldo &7in &e%{axr.timer}% secondi&7!
    deepslate_emerald_alert_msg: &7ha scavato &a%{axr_counter::%arg-1%::deepslate_emerald_ore}% blocchi di minerale di smeraldo in ardesia profonda &7in &e%{axr.timer}% secondi&7!
  # DIAMANTE
    diamond_alert_msg: &7ha scavato &b%{axr_counter::%arg-1%::diamond_ore}% blocchi di minerale di diamante &7in &e%{axr.timer}% secondi&7!
    deepslate_diamond_alert_msg: &7ha scavato &b%{axr_counter::%arg-1%::deepslate_diamond_ore}% blocchi di minerale di diamante in ardesia profonda &7in &e%{axr.timer}% secondi&7!
  # QUARZO DEL NETHER
    nether_quartz_alert_msg: &7ha scavato &f%{axr_counter::%arg-1%::nether_quartz_ore}% blocchi di minerale di quarzo del Nether &7in &e%{axr.timer}% secondi&7!
  # ORO DEL NETHER
    nether_gold_alert_msg: &7ha scavato &6%{axr_counter::%arg-1%::nether_gold_ore}% blocchi di minerale d'oro del Nether &7in &e%{axr.timer}% secondi&7!
  # DETRITI ANTICHI
    ancient_debris_alert_msg: &7ha scavato &f%{axr_counter::%arg-1%::ancient_debris}% blocchi di detriti antichi &7in &e%{axr.timer}% secondi&7!
  # MESSAGGIO NOTIFICA MINERALI
    hover_alert_msg: &7Clicca per teletrasportarti da &c%arg-1%
  # COMANDI MESSAGGIO NOTIFICA MINERALI
    alert_command1: gmsp %player%
    alert_command2: /teleport %arg-1%
    #alert_command3: v %player%
 # RILEVAMENTO AUTOMATICO MINERALI
    auto_alert_msg: &e&l%arg-1% &c&lpotrebbe star usando l'Xray!
  # COMANDI RILEVAMENTO AUTOMATICO MINERALI (KICK): questi comandi saranno eseguiti ogni qualvolta il sistema rileverà che il giocatore stia usando l'Xray.
    # MASCHIO
    auto_male_command1: kick arg-1 due to "&c--- &8[&6&lS&e&lM&6&lP &f| &4&lAnti-Xray&8] &c---%nl%%nl%&cSei stato espulso dal server perché il sistema sospetta che tu stia usando l'Xray.%nl%&7Disattivala prima di rientrare nel server!"
    auto_male_command2: nothing
    auto_male_command3: nothing
    auto_male_command4: nothing
    # FEMMINA
    auto_female_command1: kick arg-1 due to "&c--- &8[&6&lS&e&lM&6&lP &f| &4&lAnti-Xray&8] &c---%nl%%nl%&cSei stata espulsa dal server perché il sistema sospetta che tu stia usando l'Xray.%nl%&7Disattivala prima di rientrare nel server!"
    auto_female_command2: nothing
    auto_female_command3: nothing
    auto_female_command4: nothing
  # NUMERO MASSIMO DI ESPULSIONI AUTOMATICHE: superato il valore sottostante si attiveranno i comandi a seguire.
    autokicktime: 3
  # COMANDI RILEVAMENTO AUTOMATICO MINERALI: questi comandi saranno eseguiti una volta superato il valore soprastante.
    max_warns_message: &6%arg-1% &cha raggiunto il limite massimo di avvertimenti!
    auto_next_command1: nothing
    auto_next_command2: nothing
    auto_next_command3: nothing
    auto_next_command4: nothing

on load:
 # CONFIGURAZIONE TIMER
    if {axr.timer} isn't set:
        set {axr.timer} to 20
 # MINERALI SCAVATI
  # FERRO
    else if {iron_ore_mined_in_x_sec} isn't set:
        set {iron_ore_mined_in_x_sec} to 20
    else if {deepslate_iron_ore_mined_in_x_sec} isn't set:
        set {deepslate_iron_ore_mined_in_x_sec} to 20
  # RAME
    else if {copper_ore_mined_in_x_sec} isn't set:
        set {copper_ore_mined_in_x_sec} to 15
    else if {deepslate_copper_ore_mined_in_x_sec} isn't set:
        set {deepslate_copper_ore_mined_in_x_sec} to 15
  # ORO
    else if {gold_ore_mined_in_x_sec} isn't set:
        set {gold_ore_mined_in_x_sec} to 15
    else if {deepslate_gold_ore_mined_in_x_sec} isn't set:
        set {deepslate_gold_ore_mined_in_x_sec} to 15
  # REDSTONE
    else if {redstone_ore_mined_in_x_sec} isn't set:
        set {redstone_ore_mined_in_x_sec} to 16
    else if {deepslate_redstone_ore_mined_in_x_sec} isn't set:
        set {deepslate_redstone_ore_mined_in_x_sec} to 16
  # LAPISLAZZULI
    else if {lapis_ore_mined_in_x_sec} isn't set:
        set {lapis_ore_mined_in_x_sec} to 11
    else if {deepslate_lapis_ore_mined_in_x_sec} isn't set:
        set {deepslate_lapis_ore_mined_in_x_sec} to 11
  # SMERALDO
    else if {emerald_ore_mined_in_x_sec} isn't set:
        set {emerald_ore_mined_in_x_sec} to 3
    else if {deepslate_emerald_ore_mined_in_x_sec} isn't set:
        set {deepslate_emerald_ore_mined_in_x_sec} to 3
  # DIAMANTE
    else if {diamond_ore_mined_in_x_sec} isn't set:
        set {diamond_ore_mined_in_x_sec} to 6
    else if {deepslate_diamond_ore_mined_in_x_sec} isn't set:
        set {deepslate_diamond_ore_mined_in_x_sec} to 6
  # DETRITI ANTICHI
    else if {ancient_debris_mined_in_x_sec} isn't set:
        set {ancient_debris_mined_in_x_sec} to 2
  # MINERALE DI QUARZO DEL NETHER
    else if {nether_quartz_ore_mined_in_x_sec} isn't set:
        set {nether_quartz_ore_mined_in_x_sec} to 12
  # MINERALE D'ORO DEL NETHER
    else if {nether_gold_ore_mined_in_x_sec} isn't set:
        set {nether_gold_ore_mined_in_x_sec} to 16
 # RICONOSCIMENTO AUTOMATICO MINERALI SCAVATI
  # FERRO (RICONOSCIMENTO AUTOMATICO)
    else if {detect_iron_ore_mined_in_x_sec} isn't set:
        set {detect_iron_ore_mined_in_x_sec} to 30
    else if {detect_deepslate_iron_ore_mined_in_x_sec} isn't set:
        set {detect_deepslate_iron_ore_mined_in_x_sec} to 30
  # RAME (RICONOSCIMENTO AUTOMATICO)
    else if {detect_copper_ore_mined_in_x_sec} isn't set:
        set {detect_copper_ore_mined_in_x_sec} to 30
    else if {detect_deepslate_copper_ore_mined_in_x_sec} isn't set:
        set {detect_deepslate_copper_ore_mined_in_x_sec} to 30
  # ORO (RICONOSCIMENTO AUTOMATICO)
    else if {detect_gold_ore_mined_in_x_sec} isn't set:
        set {detect_gold_ore_mined_in_x_sec} to 25
    else if {detect_deepslate_gold_ore_mined_in_x_sec} isn't set:
        set {detect_deepslate_gold_ore_mined_in_x_sec} to 25
  # REDSTONE (RICONOSCIMENTO AUTOMATICO)
    else if {detect_redstone_ore_mined_in_x_sec} isn't set:
        set {detect_redstone_ore_mined_in_x_sec} to 16
    else if {detect_deepslate_redstone_ore_mined_in_x_sec} isn't set:
        set {detect_deepslate_redstone_ore_mined_in_x_sec} to 16
  # LAPISLAZZULI (RICONOSCIMENTO AUTOMATICO)
    else if {detect_lapis_ore_mined_in_x_sec} isn't set:
        set {detect_lapis_ore_mined_in_x_sec} to 11
    else if {detect_deepslate_lapis_ore_mined_in_x_sec} isn't set:
        set {detect_deepslate_lapis_ore_mined_in_x_sec} to 11
  # SMERALDO (RICONOSCIMENTO AUTOMATICO)
    else if {detect_emerald_ore_mined_in_x_sec} isn't set:
        set {detect_emerald_ore_mined_in_x_sec} to 7
    else if {detect_deepslate_emerald_ore_mined_in_x_sec} isn't set:
        set {detect_deepslate_emerald_ore_mined_in_x_sec} to 7
  # DIAMANTE (RICONOSCIMENTO AUTOMATICO)
    else if {detect_diamond_ore_mined_in_x_sec} isn't set:
        set {detect_diamond_ore_mined_in_x_sec} to 13
    else if {detect_deepslate_diamond_ore_mined_in_x_sec} isn't set:
        set {detect_deepslate_diamond_ore_mined_in_x_sec} to 13
  # DETRITI ANTICHI (RICONOSCIMENTO AUTOMATICO)
    else if {detect_ancient_debris_mined_in_x_sec} isn't set:
        set {detect_ancient_debris_mined_in_x_sec} to 7
  # MINERALE DI QUARZO DEL NETHER (RICONOSCIMENTO AUTOMATICO)
    else if {detect_nether_quartz_ore_mined_in_x_sec} isn't set:
        set {detect_nether_quartz_ore_mined_in_x_sec} to 16
  # MINERALE D'ORO DEL NETHER (RICONOSCIMENTO AUTOMATICO)
    else if {detect_nether_gold_ore_mined_in_x_sec} isn't set:
        set {detect_nether_gold_ore_mined_in_x_sec} to 21
 # CONFIGURAZIONE STATO VARIABILI SISTEMA
    if {axr.alerts} and {autokick} isn't set:
        set {axr.alerts} and {autokick} to true
    if {counter_status} and {consolenotify_status} isn't set:
        set {counter_status} and {consolenotify_status} to "{@disabled}"
    if {autokick_status}, {orealerts_status} and {autoalerts_status} isn't set:
        set {autokick_status}, {orealerts_status} and {autoalerts_status} to "{@enabled}"
    if {axr.offline_orealerts_info} isn't set:
        set {axr.offline_orealerts_info} to "&cdisattivati"
    if {axr.orealerts_info} isn't set:
        set {axr.orealerts_info} to "&aattivati"
    if {axr.counter_info} isn't set:
        set {axr.counter_info} to "&cdisattivato"
    if {axr.autoalerts_info} isn't set:
        set {axr.autoalerts_info} to "&aattivato"
    if {axr.consolenotify_info} isn't set:
        set {axr.consolenotify_info} to "&cdisattivate"
 # CONFIGURAZIONE STATO VARIABILI MINERALI
    else if {alert_iron_status}, {alert_deepslate_iron_status}, {alert_copper_status}, {alert_deepslate_copper_status}, {alert_gold_status}, {alert_deepslate_gold_status}, {alert_redstone_status}, {alert_deepslate_redstone_status}, {alert_lapis_status}, {alert_deepslate_lapis_status}, {alert_emerald_status}, {alert_deepslate_status}, {alert_diamond_status}, {alert_deepslate_diamond_status}, {alert_ancient_debris_status}, {autoalert_iron_status}, {autoalert_deepslate_iron_status}, {autoalert_copper_status}, {autoalert_deepslate_copper_status}, {autoalert_gold_status}, {autoalert_deepslate_gold_status}, {autoalert_redstone_status}, {autoalert_deepslate_redstone_status}, {autoalert_lapis_status}, {autoalert_deepslate_lapis_status}, {autoalert_emerald_status}, {autoalert_deepslate_emerald_status}, {autoalert_diamond_status}, {autoalert_deepslate_diamond_status}, {autoalert_ancient_debris_status}, {autoalert_nether_quartz_ore_status} or {autoalert_nether_gold_ore_status} isn't set:
        set {alert_iron_status}, {alert_deepslate_iron_status}, {alert_copper_status}, {alert_deepslate_copper_status}, {alert_gold_status}, {alert_deepslate_gold_status}, {alert_redstone_status}, {alert_deepslate_redstone_status}, {alert_lapis_status}, {alert_deepslate_lapis_status}, {alert_emerald_status}, {alert_deepslate_emerald_status}, {alert_diamond_status}, {alert_deepslate_diamond_status}, {alert_ancient_debris_status}, {autoalert_iron_status}, {autoalert_deepslate_iron_status}, {autoalert_copper_status}, {autoalert_deepslate_copper_status}, {autoalert_gold_status}, {autoalert_deepslate_gold_status}, {autoalert_redstone_status}, {autoalert_deepslate_redstone_status}, {autoalert_lapis_status}, {autoalert_deepslate_lapis_status}, {autoalert_emerald_status}, {autoalert_deepslate_emerald_status}, {autoalert_diamond_status}, {autoalert_deepslate_diamond_status}, {autoalert_ancient_debris_status}, {autoalert_nether_quartz_ore_status} or {autoalert_nether_gold_ore_status}, {offline_orealerts_status} to "&aattivato"
 # CONFIGURAZIONE VARIABILI MINERALI SCAVATI DAL GIOCATORE
    loop all players:
        if {axr::%loop-player%::total_ores}, {axr::%loop-player%::iron_ore}, {axr::%loop-player%::deepslate_iron_ore}, {axr::%loop-player%::copper_ore}, {axr::%loop-player%::deepslate_copper_ore}, {axr::%loop-player%::gold_ore}, {axr::%loop-player%::deepslate_gold_ore}, {axr::%loop-player%::lapis_ore}, {axr::%loop-player%::deepslate_lapis_ore}, {axr::%loop-player%::redstone_ore}, {axr::%loop-player%::deepslate_redstone_ore}, {axr::%loop-player%::emerald_ore}, {axr::%loop-player%::deepslate_emerald_ore}, {axr::%loop-player%::diamond_ore}, {axr::%loop-player%::deepslate_diamond_ore}, {axr::%loop-player%::nether_quartz_ore}, {axr::%loop-player%::nether_gold_ore} or {axr::%loop-player%::ancient_debris} isn't set:
