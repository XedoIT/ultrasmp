 # OGGETTO CHE APRE LA GUI DELL'ANTI-XRAY
on right click:
    if player has permission "axr.admin.item.use":
        if player's tool is a recovery compass named "{@prefix} &7(Click destro)":
            openAXRmain(player)
            
 # COMANDO PRINCIPALE ANTI-XRAY
command /axr [<text>] [<text>] [<text>] [<offline player>]:
    aliases: /antixray
    description: Gestisci le impostazioni dell'Anti-Xray.
    executable by: player, console
    trigger:
        if sender is player:
            if arg-1 isn't set:
                if arg-2 isn't set:
                    if arg-3 isn't set:
                        if arg-4 isn't set:
                            if player has permission "axr.admin.gui.use":
                                openAXRmain(player)
                                stop
                            else:
                                send "{@prefix_hide} &cComando sconosciuto."
                                stop
                else:
                    if player has permission "axr.admin.gui.use":
                        send "{@prefix} &cQuesto comando non richiede parametri aggiuntivi."
                        stop
                    else:
                        send "{@prefix_hide} &cComando sconosciuto."
                        stop
                        
            else if arg-1 is "item":
                if arg-2 isn't set:
                    if arg-3 isn't set:
                        if arg-4 isn't set:
                            give a recovery compass named "{@prefix} &7(Click destro)" to player
                            stop

            else if arg-1 is "world":
                if arg-2 isn't set:
                    if arg-3 isn't set:
                        if arg-4 isn't set:
                            if player has permission "axr.admin.world":
                                send "{@prefix} &cComando incompleto."
                                send "{@prefix} &cUtilizzo: &6/axr world add <&emondo&6>"
                                send "{@prefix} &cUtilizzo: &6/axr world remove <&emondo&6>"
                                stop
                            else:
                                send "{@prefix_hide} &cComando sconosciuto."
                                stop
                else if arg-2 is "add":
                    if arg-3 isn't set:
                        if arg-4 isn't set:
                            if player has permission "axr.admin.world.add":
                                send "{@prefix} &3Mondi disponibili: &b%{axr::all_worlds::*}%"
                                send "{@prefix} &cUtilizzo: &6/axr world add <&emondo&6>"
                                stop
                            else:
                                send "{@prefix_hide} &cComando sconosciuto."
                                stop
                    else:
                        if arg-4 isn't set:
                            if player has permission "axr.admin.world.add":
                                set {add_world} to arg-3 parsed as world
                                if {axr::worlds::*} is "&7Nessun mondo abilitato.":
                                    delete {axr::worlds::*}
                                    send "{@prefix} &aHai aggiunto &b%{add_world}% &aalla lista dei mondi abilitati!"
                                    add {add_world} to {axr::worlds::*}
                                    remove {add_world} from {axr::all_worlds::*}
                                    if {axr::all_worlds::*} isn't set:
                                        set {axr::all_worlds::*} to "&7Nessun mondo disponibile."
                                    play sound "{@confirm_sound}" with volume 1 and pitch 2 at player for player   
                                    wait 5 ticks
                                    stop
                                else if {axr::worlds::*} contains {add_world}:
                                    send "{@prefix} &cIl mondo &6%{add_world}% &cè già stato aggiunto!"
                                    play sound "{@error_sound}" with volume 1 and pitch 0 at player for player
                                    stop
                                else if {axr::all_worlds::*} doesn't contain {add_world}:
                                    send "{@prefix} &cNon esiste nessun mondo con quel nome!"
                                    play sound "{@error_sound}" with volume 1 and pitch 0 at player for player
                                    stop
                                else if {axr::worlds::*} doesn't contain {add_world}:
                                    send "{@prefix} &aHai aggiunto &b%{add_world}% &aalla lista dei mondi abilitati!"
                                    add {add_world} to {axr::worlds::*}
                                    remove {add_world} from {axr::all_worlds::*}
                                    if {axr::all_worlds::*} isn't set:
                                        set {axr::all_worlds::*} to "&7Nessun mondo disponibile."
                                    play sound "{@confirm_sound}" with volume 1 and pitch 2 at player for player
                                    wait 5 ticks
                                    stop
                            else:
                                send "{@prefix_hide} &cComando sconosciuto."
                                stop
                        else:
                            if player has permission "axr.admin.world.add":
                                send "{@prefix} &cQuesto comando non richiede parametri aggiuntivi."
                                stop
                            else:
                                send "{@prefix_hide} &cComando sconosciuto."
                                stop

                else if arg-2 is "remove":
                    if arg-3 isn't set:
                        if arg-4 isn't set:
                            if player has permission "axr.admin.world.remove":
                                send "{@prefix} &4Puoi rimuovere: &6%{axr::worlds::*}%"
                                send "{@prefix} &cUtilizzo: &6/axr world remove <&emondo&6>"
                                stop
                            else:
                                send "{@prefix_hide} &cComando sconosciuto."
                                stop
                    else:
                        if arg-4 isn't set:
                            if player has permission "axr.admin.world.remove":
                                set {removed_world} to arg-3 parsed as world
                                if {axr::worlds::*} contains {removed_world}:
                                    if {axr::all_worlds::*} is "&7Nessun mondo disponibile.":
                                        delete {axr::all_worlds::*}
                                        send "{@prefix} &aHai rimosso &b%{removed_world}% &adalla lista dei mondi abilitati!"
                                        play sound "{@confirm_sound}" with volume 1 and pitch 2 at player for player   
                                        wait 5 ticks
                                        remove {removed_world} from {axr::worlds::*}
                                        if {axr::worlds::*} isn't set:
                                            set {axr::worlds::*} to "&7Nessun mondo abilitato."
                                        add {removed_world} to {axr::all_worlds::*}
                                        stop
                                    else if {axr::all_worlds::*} isn't "&7Nessun mondo disponibile.":
                                        send "{@prefix} &aHai rimosso &b%{removed_world}% &adalla lista dei mondi abilitati!"
                                        play sound "{@confirm_sound}" with volume 1 and pitch 2 at player for player   
                                        wait 5 ticks
                                        remove {removed_world} from {axr::worlds::*}
                                        if {axr::worlds::*} isn't set:
                                            set {axr::worlds::*} to "&7Nessun mondo abilitato."
                                        add {removed_world} to {axr::all_worlds::*}
                                        stop
                                else:
                                    if {axr::all_worlds::*} doesn't contain {removed_world}:
                                        send "{@prefix} &cNon esiste nessun mondo con quel nome!"
                                        play sound "{@error_sound}" with volume 1 and pitch 0 at player for player
                                        stop
                                    send "{@prefix} &cIl mondo &6%{removed_world}% &cnon è nella lista dei mondi abilitati!"
                                    play sound "{@error_sound}" with volume 1 and pitch 0 at player for player
                                    stop
                                    
                            else:
                                send "{@prefix_hide} &cComando sconosciuto."
                                stop
                        else:
                            if player has permission "axr.admin.world.remove":
                                send "{@prefix} &cQuesto comando non richiede parametri aggiuntivi."
                                stop
                            else:
                                send "{@prefix_hide} &cComando sconosciuto."
                                stop
                else if arg-2 isn't "add" or "remove":
                    if player has permission "axr.admin.world":
                        send "{@prefix} &cComando sconosciuto."
                        send "{@prefix} &cUtilizzo: &6/axr world add <&emondo&6>"
                        send "{@prefix} &cUtilizzo: &6/axr world remove <&emondo&6>"
                        stop
                    else:
                        send "{@prefix_hide} &cComando sconosciuto."
                        stop

            else if arg-1 is "ore":
                if arg-2 isn't set:
                    if arg-3 isn't set:
                        if arg-4 isn't set:
                            if player has permission "axr.admin.ore":
                                send "{@prefix} &cComando incompleto."
                                send "{@prefix} &cUtilizzo: &6/axr ore view for <&egiocatore&6>"
                                send "{@prefix} &cUtilizzo: &6/axr ore counter"
                                send "{@prefix} &cUtilizzo: &6/axr ore clear all <&egiocatore&6>"
                                send "{@prefix} &cUtilizzo: &6/axr ore alerts all"
                                stop
                            else:
                                send "{@prefix_hide} &cComando sconosciuto."
                                stop
                else if arg-2 is "view" or "check":
                    if arg-3 isn't set:
                        if arg-4 isn't set:
                            if player has permission "axr.admin.ore.viewer":
                                send "{@prefix} &cComando incompleto."
                                send "{@prefix} &cUtilizzo: &6/axr ore view for <&egiocatore&6>"
                                play sound "{@error_sound}" with volume 1 and pitch 0 at player for player
                                stop
                            else:
                                send "{@prefix_hide} &cComando sconosciuto."
                                stop
                    else if arg-3 is "for":
                        if arg-4 isn't set:
                            if player has permission "axr.admin.ore.viewer":
                                send "{@prefix} &cSpecifica il nome di un giocatore!"
                                play sound "{@error_sound}" with volume 1 and pitch 0 at player for player
                                stop
                            else:
                                send "{@prefix_hide} &cComando sconosciuto."
                                stop
                        else if arg-4 is online:
                            if {axr.counter} is true:
                                send "{@prefix} &7Sto controllando i minerali scavati di &c%arg-4%&7..."
                                play sound "{@check_sound}" with volume 1 at player for player
                                wait 1 second
                                delete {checkplayer.latest}
                                set {checkplayer.latest} to arg-4
                                set {arg4} to arg-4
                                openAXRores(player, arg-4)
                                stop
                            else:
                                send "{@prefix} &cIl contatore dei minerali non è attivato!"
                                send "{@prefix} &eAttivalo utilizzando &6/axr settings counter"
                                play sound "{@error_sound}" with volume 1 and pitch 0 at player for player
                                stop
                        else if arg-4 has played before:
                            if {axr.counter} is true:
                                send "{@prefix} &7Sto controllando i minerali scavati di &c%arg-4%&7..."
