# Script per il sistema dei punti, reputazioni ed effetti dati dalle reputazioni #

#PlaceHolder per i PUNTI: %skript_points::{player}%
#PlaceHolder per gli EFFETTI: %skript_points::{player}::effect%

#Impostazioni per Punti e Reputazioni
options:
	prefix1: &8[&b&lPUNTI&8]
	error_prefix1: &8[&4&lPUNTI&8]
	prefix2: &8[&b&lRANK&8]
	error_prefix2: &8[&4&lRANK&8]
	version: 1.0

# Evento per Reputazioni
on load:
	loop all players:
		set {_alignment-5} to "&4&lMalvagio/a"
		set {_alignment-4} to "&c&lPessimo/a"
		set {_alignment-3} to "&6&lVile"
		set {_alignment-2} to "&e&lSpregevole"
		set {_alignment-1} to "&8&lAggressivo/a"
		set {_alignment0} to "&7&lNeutrale"
		set {_alignment1_1} to "&a&lGentile"
		set {_alignment1_2} to "&a&lGentile &f&l+"
		set {_alignment2_1} to "&2&lBuono/a"
		set {_alignment2_2} to "&2&lBuono/a &f&l+"
		set {_alignment3_1} to "&3&lPuro/a"
		set {_alignment3_2} to "&3&lPuro/a &f&l+"
		set {_alignment4_1} to "&9&lMigliore"
		set {_alignment4_2} to "&9&lMigliore &f&l+"
		set {_alignment5_1} to "&b&lSupremo/a"
		set {_alignment5_2} to "&b&lSupremo/a &f&l+"
		set {points::%loop-player%::effect} to "&7Nessun effetto"
		set {points::%loop-player%::rankprefix} to "&8[&7&lN&8]"

# Evento per Punti
variables:
	{points::%player%} = 0
	{points::%player%::rank} = "&7&lNeutrale"
	{points::%player%::rankprefix} = "&8[&7&lN&8]"
	{points::%player%::idle} = true

# Evento per Punti
on join:
	if {points::%player%} is not set:
		set {points::%player%} to 0
	if {points::%player%::rank} is not set:
		set {points::%player%::rank} to "&7&lNeutrale"
	if {points::%player%::rankprefix} is not set:
		set {points::%player%::rankprefix} to "&8[&7&lN&8]"
	if {points::%player%::idle} is not set:
		set {points::%player%::idle} to true
#--------------------------------------------#
#											 #
#				   GENERALE					 #
#											 #
#--------------------------------------------#
every 5 hours:
	set {_alignment-5} to "&4&lMalvagio/a"
	set {_alignment-4} to "&c&lPessimo/a"
	set {_alignment-3} to "&6&lVile"
	set {_alignment-2} to "&e&lSpregevole"
	set {_alignment-1} to "&8&lAggressivo/a"
	set {_alignment0} to "&7&lNeutrale"
	set {_alignment1_1} to "&a&lGentile"
	set {_alignment1_2} to "&a&lGentile &f&l+"
	set {_alignment2_1} to "&2&lBuono/a"
	set {_alignment2_2} to "&2&lBuono/a &f&l+"
	set {_alignment3_1} to "&3&lPuro/a"
	set {_alignment3_2} to "&3&lPuro/a &f&l+"
	set {_alignment4_1} to "&9&lMigliore"
	set {_alignment4_2} to "&9&lMigliore &f&l+"
	set {_alignment5_1} to "&b&lSupremo/a"
	set {_alignment5_2} to "&b&lSupremo/a &f&l+"
	set {_points-5} to a random integer between 300 and 400
	set {_points-4} to a random integer between 250 and 300
	set {_points-3} to a random integer between 200 and 250
	set {_points-2} to a random integer between 100 and 200
	set {_points-1} to a random integer between 50 and 100
	set {_points0} to a random integer between 10 and 50
	set {_points1_1} to a random integer between 50 and 75
	set {_points1_2} to a random integer between 50 and 125
	set {_points2_1} to a random integer between 125 and 175
	set {_points2_2} to a random integer between 125 and 200
	set {_points3_1} to a random integer between 225 and 250
	set {_points3_2} to a random integer between 225 and 275
	set {_points4_1} to a random integer between 250 and 325
	set {_points4_2} to a random integer between 250 and 350
	set {_points5_1} to a random integer between 350 and 450
	set {_points5_2} to a random integer between 450 and 500
	loop all players:
		if loop-player is online:
			if {points::%loop-player%} is lower than 1000000:
				if {points::%loop-player%::idle} is true:
					if {points::%loop-player%::rank} is {_alignment-5}:
						add {_points-5} to {points::%loop-player%}
						send "&6+%{_points-5}% punti! (Tempo di gioco) &b&l>> &6(%{_alignment-5}%&6)" to loop-player
						stop
					else if {points::%loop-player%::rank} is {_alignment-4}:
						add {_points-4} to {points::%loop-player%}
						send "&6+%{_points-4}% punti! (Tempo di gioco) &b&l>> &6(%{_alignment-4}%&6)" to loop-player
						stop
					else if {points::%loop-player%::rank} is {_alignment-3}:
						add {_points-3} to {points::%loop-player%}
						send "&6+%{_points-3}% punti! (Tempo di gioco) &b&l>> &6(%{_alignment-3}%&6)" to loop-player
						stop
					else if {points::%loop-player%::rank} is {_alignment-2}:
						add {_points-2} to {points::%loop-player%}
						send "&6+%{_points-2}% punti! (Tempo di gioco) &b&l>> &6(%{_alignment-2}%&6)" to loop-player
						stop
					else if {points::%loop-player%::rank} is {_alignment-1}:
						add {_points-1} to {points::%loop-player%}
						send "&6+%{_points-1}% punti! (Tempo di gioco) &b&l>> &6(%{_alignment-1}%&6)" to loop-player
						stop
					else if {points::%loop-player%::rank} is {_alignment0}:
						add {_points0} to {points::%loop-player%}
						send "&6+%{_points0}% punti! (Tempo di gioco) &b&l>> &6(%{_alignment0}%&6)" to loop-player
						stop
					else if {points::%loop-player%::rank} is {_alignment1_1}:
						add {_points1_1} to {points::%loop-player%}
						send "&6+%{_points1_1}% punti! (Tempo di gioco) &b&l>> &6(%{_alignment1_1}%&6)" to loop-player
						stop
					else if {points::%loop-player%::rank} is {_alignment1_2}:
						add {_points1_2} to {points::%loop-player%}
						send "&6+%{_points1_2}% punti! (Tempo di gioco) &b&l>> &6(%{_alignment1_2}%&6)" to loop-player
						stop
					else if {points::%loop-player%::rank} is {_alignment2_1}:
						add {_points2_1} to {points::%loop-player%}
						send "&6+%{_points2_1}% punti! (Tempo di gioco) &b&l>> &6(%{_alignment2_1}%&6)" to loop-player
						stop
					else if {points::%loop-player%::rank} is {_alignment2_2}:
						add {_points2_2} to {points::%loop-player%}
						send "&6+%{_points2_2}% punti! (Tempo di gioco) &b&l>> &6(%{_alignment2_2}%&6)" to loop-player
						stop
					else if {points::%loop-player%::rank} is {_alignment3_1}:
						add {_points3_1} to {points::%loop-player%}
						send "&6+%{_points3_1}% punti! (Tempo di gioco) &b&l>> &6(%{_alignment3_1}%&6)" to loop-player
						stop
					else if {points::%loop-player%::rank} is {_alignment3_2}:
						add {_points3_2} to {points::%loop-player%}
						send "&6+%{_points3_2}% punti! (Tempo di gioco) &b&l>> &6(%{_alignment3_2}%&6)" to loop-player
						stop
					else if {points::%loop-player%::rank} is {_alignment4_1}:
						add {_points4_1} to {points::%loop-player%}
						send "&6+%{_points4_1}% punti! (Tempo di gioco) &b&l>> %{_alignment4_1}%&6)" to loop-player
						stop
					else if {points::%loop-player%::rank} is {_alignment4_2}:
						add {_points4_2} to {points::%loop-player%}
						send "&6+%{_points4_2}% punti! (Tempo di gioco) &b&l>> (%{_alignment4_2}%&6)" to loop-player
						stop
					else if {points::%loop-player%::rank} is {_alignment5_1}:
						add {_points5_1} to {points::%loop-player%}
						send "&6+%{_points5_1}% punti! (Tempo di gioco) &b&l>> (%{_alignment5_1}%&6)" to loop-player
						stop
					else if {points::%loop-player%::rank} is {_alignment5_2}:
						add {_points5_2} to {points::%loop-player%}
						send "&6+%{_points5_2}% punti! (Tempo di gioco) &b&l>> (%{_alignment5_2}%&6)" to loop-player
						stop
				else:
					if loop-player has permission "points.idle.notify":
						send "{@error_prefix1} &cNon posso assegnare i punti ai giocatori perché l'assegnazione automatica dei punti è disattivata!" to loop-player
						send "&cPer attivarla, digita &6/idle on&c." to loop-player
						stop
					else:
						stop
			else:
				send "&6Hai raggiunto la reputazione più alta del server, pertanto non riceverai più punti in modo automatico." to loop-player
				send "&bPuoi comunque guadagnarli giocando normalmente nel server!" to loop-player
				stop
		else:
			stop
		
command /points [<text>] [<offline player>] [<number>]:
	aliases: /punti, /p
	description: Visualizza quanti punti hai accumulato.
	executable by: player, console
	trigger:
		if sender is player:
			if arg-1 is not set:
				if {points::%player%} is 1:
					send "{@prefix1} &6Hai accumulato &c%{points::%player%}% punto&6."
					stop
				else if {points::%player%} is -1:
					send "{@prefix1} &6Hai accumulato &c%{points::%player%}% punto&6."
					stop
				if {points::%player%} is 0:
					send "{@prefix1} &6Hai &c%{points::%player%}% punti&6."
					stop
				if {points::%player%} is not "-1", "0" or "1":
					send "{@prefix1} &6Hai accumulato &c%{points::%player%}% punti&6."
					stop
				
			else if arg-1 is not "check", "reset", "set", "give", "add", "remove", "auto", "help", "info" or "reload":
				if player has permission "points.commands":
					send "{@error_prefix1} &cComando sconosciuto."
					stop
				else:
					send "{@error_prefix1} &cNon hai i permessi per farlo!"
					stop
					
			else if arg-1 is "check":
				if player has permission "points.check":
					if arg-2 is set:
						if {points::%arg-2%} is set:
							send "{@prefix1} &c%arg-2% &6ha accumulato &e%{points::%arg-2%}% punti&6."
							stop
						else:
							send "{@error_prefix1} &6%arg-2% &cnon ha ancora giocato in questo server!"
							stop
					else:
						send "{@error_prefix1} &cSpecifica il nome di un giocatore!"
						stop
				else:
					send "{@error_prefix1} &cNon hai i permessi per farlo!"
					stop
					
			else if arg-1 is "reset":
				if player has permission "points.reset":
					if arg-2 is set:
						if {points::%arg-2%} is not 0:
							set {points::%arg-2%} to 0
							send "{@prefix1} &6Hai azzerato i punti di &c%arg-2%&6." to player
							send "{@prefix1} &c%player% &6ha azzerato i tuoi punti." to arg-2
							stop
						else:
							send "{@error_prefix1} &6%arg-2% &cnon ha nessun punto!"
							stop
							
					else if arg-2 is not set:
						if {points::%player%} is not 0:
							set {points::%player%} to 0 
							send "{@prefix1} &6Hai azzerato i tuoi punti."
							stop
						else:
							send "{@error_prefix1} &cNon hai nessun punto!"
							stop	
				else:
					send "{@error_prefix1} &cNon hai i permessi per farlo!"
					stop
					
			else if arg-1 is "set":
				if player has permission "points.set":
					if arg-2 is set:
						if arg-3 is set:
							if arg-3 is 0:
								set {points::%arg-2%} to arg-3
								send "{@prefix1} &6Hai impostato i punti di &c%arg-2% &6a &e%arg-3%&6." to player
								send "{@prefix1} &c%player% &6ha impostato i tuoi punti a &e%arg-3%&6." to arg-2
								stop
							if arg-3 is higher than 0:
								set {points::%arg-2%} to arg-3
								send "{@prefix1} &6Hai impostato i punti di &c%arg-2% &6a &e%arg-3%&6." to player
								send "{@prefix1} &c%player% &6ha impostato i tuoi punti a &e%arg-3%&6." to arg-2
								stop
							else:
								send "{@error_prefix1} &cIl numero deve essere positivo!"
								stop
						else:
							send "{@error_prefix1} &cSpecifica un numero!"
							stop
					else:
						send "{@error_prefix1} &cSpecifica il giocatore a cui vuoi assegnare i punti!"
						stop		
				else:
					send "{@error_prefix1} &cNon hai i permessi per farlo!"
					stop
				
			else if arg-1 is "add" or "give":
				if player has permission "points.add":
					if arg-2 is set:
						if arg-3 is set:
							if arg-3 is 0:
								send "{@error_prefix1} &cChe senso ha aggiungere 0 punti?"
								stop
							if arg-3 is higher than 0:
								add arg-3 to {points::%arg-2%}
								send "{@prefix1} &6Hai dato &e%arg-3% punti &6a &c%arg-2%&6." to player
								send "{@prefix1} &c%player% &6ti ha dato &e%arg-3% punti &6in più." to arg-2
								stop
							else:
								send "{@error_prefix1} &cIl numero deve essere positivo!"
								stop
						else:
							send "{@error_prefix1} &cSpecifica un numero!"
							stop
					else:
						send "{@error_prefix1} &cSpecifica il giocatore a cui vuoi aggiungere i punti!"
						stop	
				else:
					send "{@error_prefix1} &cNon hai i permessi per farlo!"
					stop
				
			else if arg-1 is "remove":
				if player has permission "points.remove":
					if arg-2 is set:
						if arg-3 is set:
							if arg-3 is 0:
								send "{@error_prefix1} &cChe senso ha togliere 0 punti?"
								stop
							if arg-3 is higher than 0:
								remove arg-3 from {points::%arg-2%}
								send "{@prefix1} &6Hai tolto &e%arg-3% punti &6a &c%arg-2%&6." to player
								send "{@prefix1} &c%player% &6ti ha tolto &e%arg-3% punti&6." to arg-2
								stop
							else:
								send "{@error_prefix1} &cIl numero deve essere positivo!"
								stop
						else:
							send "{@error_prefix1} &cSpecifica un numero!"
							stop
					else:
						send "{@error_prefix1} &cSpecifica il giocatore a cui vuoi togliere i punti!"
						stop
				else:
					send "{@error_prefix1} &cNon hai i permessi per farlo!"
					stop
					
			else if arg-1 is "help":
				if player has permission "points.help":
					send "&e> &7&m------------&r {@prefix1} &7&m------------&r &e<"
					send ""
					send "&7Comandi che puoi eseguire:"
					send "&c- &a/points &7&o- Visualizza quanti punti hai accumulato."
					send "&c- &7/points &acheck <giocatore> &7&o- Controlla quanti punti ha un giocatore"
					send "&c- &7/points &areset <giocatore> &7&o- Azzera i tuoi punti o quelli di un giocatore"
					send "&c- &7/points &aset | give <giocatore> <punti> &7&o- Imposta il valore dei punti di un giocatore"
					send "&c- &7/points &aadd <giocatore> <punti> &7&o- Aggiungi dei punti ad un giocatore"
					send "&c- &7/points &aremove <giocatore> <punti> &7&o- Rimuovi dei punti ad un giocatore"
					send "&c- &7/points &aauto <giocatore> &7&o- Assegna in modo automatico i punti ad un giocatore rispettando la sua reputazione"
					send "&c- &7/points &ainfo &7&o- Visualizza le info dello script"
					send "&c- &7/points &ahelp &7&o- Mostra questa lista comandi"
					send "&c- &7/points &areload &7&o- Ricarica lo script"
					send ""
					send "&e> &7&m--------------------------------&r &e<"
					stop
				else:
					send "&e> &7&m------------&r {@prefix1} &7&m------------&r &e<"
					send ""
					send "&7Comandi che puoi eseguire:"
					send "&c- &a/points &7&o- Visualizza quanti punti hai accumulato"
					send "&c- &7/points &ainfo &7&o- Visualizza le info dello script"
					send "&c- &7/points &ahelp &7&o- Mostra questa lista comandi"
					send ""
					send "&e> &7&m--------------------------------&r &e<"
					stop
				
			else if arg-1 is "info":
				send "&a> &9&m------------&r {@prefix1} &9&m------------&r &a<"
				send ""
				send " &7Info sullo script:"
				send "	 &3Versione dello script: &c{@version}"
				send "	 &3Versione di Minecraft: &a%minecraft version%"
				send "	 &3Script ideato e scritto da &bXedo"
				send ""
				send "&a> &9&m--------------------------------&r &a<"
				stop
			
			else if arg-1 is "reload":
				if player has permission "points.reload":
					send "{@prefix1} &3Ricarico il config principale..."
					wait 5 ticks
					execute player command "/skript reload points (comandi)"
					wait 2 seconds
					send "{@prefix1} &3Ricarico il config dei MINERALI..."
					wait 5 ticks
					execute player command "/skript reload points (minerali)"
					wait 2 seconds
					send "{@prefix1} &3Ricarico il config dei MOB..."
					wait 5 ticks
					execute player command "/skript reload points (mob)"
					wait 2 seconds
					send "{@prefix1} &3Ricarico il config delle REPUTAZIONI..."
					wait 5 ticks
					execute player command "/skript reload points (alignment)"
					wait 2 seconds
					send "{@prefix1} &3Ricarico il config degli EFFETTI..."
					execute player command "/skript reload points (effects)"
					wait 3 seconds
					send "{@prefix1} &aConfig ricaricati!"
					stop
				else:
					send "{@error_prefix1} &cNon hai i permessi per farlo!"
					stop
				
		if sender is console:
			if arg-1 is not set:
				send "{@error_prefix1} &cQuesto comando può essere eseguito solo dai giocatori!"
				stop
				
			if arg-1 is not "check", "reset", "set", "give", "add", "remove", "auto", "help", "info" or "reload":
				send "{@error_prefix1} &cComando sconosciuto."
				stop
					
			if arg-1 is "check":
				if arg-2 is set:
					if {points::%arg-2%} is set:
						send "{@prefix1} &c%arg-2% &6ha accumulato &e%{points::%arg-2%}% punti&6."
						stop
					else:
						send "{@error_prefix1} &6%arg-2% &cnon ha ancora giocato in questo server!"
						stop
				else:
					send "{@error_prefix1} &cSpecifica il nome di un giocatore!"
					stop	
					
			if arg-1 is "reset":
				if arg-2 is set:
					if {points::%arg-2%} is not 0:
						set {points::%arg-2%} to 0
						send "{@prefix1} &6Hai azzerato i punti di &c%arg-2%&6." to console
						send "{@prefix1} &6La &cconsole &6ha azzerato i tuoi punti." to arg-2
						stop
					else:
						send "{@error_prefix1} &6%arg-2% &cnon ha nessun punto!"
						stop
				else:
					send "{@error_prefix1} &cQuesta funzione la possono utilizzare solo i giocatori!"
					stop	
					
			if arg-1 is "set":
				if arg-2 is set:
					if arg-3 is set:
						if arg-3 is 0:
							set {points::%arg-2%} to arg-3
							send "{@prefix1} &6Hai impostato i punti di &c%arg-2% &6a &e%arg-3%&6." to player
							send "{@prefix1} &c%player% &6ha impostato i tuoi punti a &e%arg-3%&6." to arg-2
							stop
						if arg-3 is higher than 0:
							set {points::%arg-2%} to arg-3
							send "{@prefix1} &6Hai impostato i punti di &c%arg-2% &6a &e%arg-3%&6." to console
							send "{@prefix1} &6La &cconsole &6ha impostato i tuoi punti a &e%arg-3%&6." to arg-2
							stop
						else:
							send "{@error_prefix1} &cIl numero deve essere positivo!"
							stop
					else:
						send "{@error_prefix1} &cSpecifica un numero!"
						stop
				else:
					send "{@error_prefix1} &cSpecifica il giocatore a cui vuoi assegnare i punti!"
					stop	
					
			if arg-1 is "add":
				if arg-2 is set:
					if arg-3 is set:
						if arg-3 is 0:
							send "{@error_prefix1} &cChe senso ha aggiungere 0 punti?"
							stop
						if arg-3 is higher than 0:
							add arg-3 to {points::%arg-2%}
							send "{@prefix1} &6Hai dato &e%arg-3% punti &6a &c%arg-2%&6." to console
							send "{@prefix1} &6La &cconsole &6ti ha dato &e%arg-3% punti &6in più." to arg-2
							stop
						else:
							send "{@error_prefix1} &cIl numero deve essere positivo!"
							stop
					else:
						send "{@error_prefix1} &cSpecifica un numero!"
						stop
				else:
					send "{@error_prefix1} &cSpecifica il giocatore a cui vuoi aggiungere i punti!"
					stop
		
			if arg-1 is "remove":
				if arg-2 is set:
					if arg-3 is set:
						if arg-3 is 0:
							send "{@error_prefix1} &cChe senso ha togliere 0 punti?"
							stop
						if arg-3 is higher than 0:
							remove arg-3 from {points::%arg-2%}
							send "{@prefix1} &6Hai tolto &e%arg-3% punti &6a &c%arg-2%&6." to console
							send "{@prefix1} &6La &cconsole &6ti ha tolto &e%arg-3% punti&6." to arg-2
							stop
						else:
							send "{@error_prefix1} &cIl numero deve essere positivo!"
							stop
					else:
						send "{@error_prefix1} &cSpecifica un numero!"
						stop
				else:
