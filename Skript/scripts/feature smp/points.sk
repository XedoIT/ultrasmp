command /rank [<text>] [<offline player>] [<text>]:
	aliases: /alignment, /reputazione
	description: Gestisci la reputazione di un giocatore.
	executable by: player, console
	trigger:
		set {_alignment-5} to "&4&lMalvagio/a"
		set {_alignment-4} to "&c&lPessimo/a"
		set {_alignment-3} to "&6&lVile"
		set {_alignment-2} to "&e&lSpregevole"
		set {_alignment-1} to "&8&lAggressivo/a"
		set {_alignment0} to "&7&lNeutrale"
		set {_alignment1_1} to "&a&lGentile"
		set {_alignment1_2} to "&a&lGentile &f&l+"
		set {_alignment2_1} to "&2&lBuono/a"
		set {_alignment2_2} to "&2&lBuono/a &f&l+"
		set {_alignment3_1} to "&3&lPuro/a"
		set {_alignment3_2} to "&3&lPuro/a &f&l+"
		set {_alignment4_1} to "&9&lMigliore"
		set {_alignment4_2} to "&9&lMigliore &f&l+"
		set {_alignment5_1} to "&b&lSupremo/a"
		set {_alignment5_2} to "&b&lSupremo/a &f&l+"
		if sender is player:
			if arg-1 is not set:
				if arg-2 is not set:
					if arg-3 is not set:
						send "{@prefix2} &fLa tua reputazione: %{points::%player%::rank}%"
						stop
			
			else if arg-1 is "check":
				if player has permission "alignment.check":
					if arg-2 is set:
						if {points::%arg-2%::rank} is set:
							send "{@prefix2} &fReputazione di %arg-2%: %{points::%arg-2%::rank}%"
							stop
						else:
							send "{@error_prefix2} &6%arg-2% &cnon possiede ancora nessun ruolo!"
							stop
					else:
						send "{@error_prefix2} &cSpecifica il nome di un giocatore!"
						stop
						if arg-3 is set:
							send "{@error_prefix2} &cHai inserito troppi parametri!"
							stop
				else:
					send "{@error_prefix2} &cNon hai i permessi per farlo!"
					stop
					
			else if arg-1 is "set":
				if player has permission "alignment.set":
