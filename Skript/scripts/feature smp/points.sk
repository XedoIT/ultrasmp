command /idle [<text>]:
	permission: points.idle.toggle
	permission message: &8[&4&lINFO&8] &cComando sconosciuto.
	description: Attiva o disattiva l'assegnazione automatica dei punti ai giocatori.
	executable by: player, console
	trigger:
		if sender is player:
			if arg-1 is not set:
				send "&cUtilizzo: &6/idle &e<on | off>"
				stop
			if arg-1 is "on":
				if {points::%player%::idle} is true:
					send "{@error_prefix1} &cL'assegnazione automatica dei punti è già attivata!"
					stop
				else:
					set {points::%player%::idle} to true
					send "{@prefix1} &aAssegnazione automatica dei punti attivata!"
					stop
			else if arg-1 is "off":
				if {points::%player%::idle} is false:
					send "{@error_prefix1} &cL'assegnazione automatica dei punti è già disattivata!"
					stop
				else:
					set {points::%player%::idle} to false
					send "{@prefix1} &cAssegnazione automatica dei punti disattivata!"
					stop
		else:
			if arg-1 is not set:
				send "&cUtilizzo: &6/idle &e<on | off>"
				stop
			if arg-1 is "on":
				if {points::%player%::idle} is true:
					send "{@error_prefix1} &cL'assegnazione automatica dei punti è già attivata!"
					stop
				else:
					set {points::%player%::idle} to true
