 # OGGETTO CHE APRE LA GUI DELL'ANTI-XRAY (DA RIMUOVERE AL COMPLETAMENTO DELLO SCRIPT!!)
on right click:
    loop all players:
        if player's tool is a recovery compass named "{@prefix} &7(Click destro)":
            openAXRmain(player)
 # COMANDO PRINCIPALE ANTI-XRAY
command /axr [<text>] [<text>] [<text>] [<offline player>]:
    aliases: /antixray
    description: Gestisci le impostazioni dell'Anti-Xray.
    executable by: player, console
    trigger:
        if sender is player:
            if arg-1 is not set:
                if arg-2 is not set:
                    if arg-3 is not set:
                        if arg-4 is not set:
                            if player has permission "axr.admin.gui.use":
                                openAXRmain(player)
                                stop
                            else:
                                send "{@prefix_hide} &cComando sconosciuto."
                                stop
                else:
                    if player has permission "axr.admin.gui.use":
                        send "{@prefix} &cQuesto comando non richiede parametri aggiuntivi."
                        stop
                    else:
                        send "{@prefix_hide} &cComando sconosciuto."
                        stop
            # PARAMETRO DA RIMUOVERE AL COMPLETAMENTO DELLO SCRIPT!!
            else if arg-1 is "item":
                if arg-2 is not set:
                    if arg-3 is not set:
                        if arg-4 is not set:
                            give a recovery compass named "{@prefix} &7(Click destro)" to player
                            stop

            else if arg-1 is "world":
                if arg-2 is not set:
                    if arg-3 is not set:
                        if arg-4 is not set:
                            if player has permission "axr.admin.world":
                                send "{@prefix} &cComando incompleto."
                                send "{@prefix} &cUtilizzo: &6/axr world <&eadd &6| &eremove&6>"
                                stop
                            else:
                                send "{@prefix_hide} &cComando sconosciuto."
                                stop
                else if arg-2 is "add":
                    if arg-3 is not set:
                        if arg-4 is not set:
                            if player has permission "axr.admin.world.add":
                                send "{@prefix} &aMondi disponibili: &b%{all_worlds::*}%"
                                send "{@prefix} &cUtilizzo: &6/axr world add <&emondo&6>"
                                stop
                            else:
                                send "{@prefix_hide} &cComando sconosciuto."
                                stop
                    else if arg-3 is "smp", "smp_nether" or "smp_the_end":
                        if arg-4 is not set:
                            if player has permission "axr.admin.world.add":
                                set {add_world} to arg-3 parsed as world
                                if {axr_worlds::*} is "&7Nessun mondo abilitato.":
                                    delete {axr_worlds::*}
                                    send "{@prefix} &aHai aggiunto &b%{add_world}% &aalla lista dei mondi abilitati!"
                                    add {add_world} to {axr_worlds::*}
                                    remove {add_world} from {all_worlds::*}
                                    if {all_worlds::*} is not set:
                                        set {all_worlds::*} to "&7Nessun mondo disponibile."
                                    play sound "{@confirm_sound}" with volume 1 and pitch 2 at player for player
                                    stop
                                else if {axr_worlds::*} contains {add_world}:
                                    send "{@prefix} &cIl mondo &6%{add_world}% &cè già stato aggiunto!"
                                    play sound "{@error_sound}" with volume 1 and pitch 0 at player for player
                                    stop
                                else if {axr_worlds::*} doesn't contain {add_world}:
                                    send "{@prefix} &aHai aggiunto &b%{add_world}% &aalla lista dei mondi abilitati!"
                                    add {add_world} to {axr_worlds::*}
                                    remove {add_world} from {all_worlds::*}
                                    if {all_worlds::*} is not set:
                                        set {all_worlds::*} to "&7Nessun mondo disponibile."
                                    play sound "{@confirm_sound}" with volume 1 and pitch 2 at player for player
                                    stop
                            else:
                                send "{@prefix_hide} &cComando sconosciuto."
                                stop
                        else:
                            if player has permission "axr.admin.world.add":
                                send "{@prefix} &cQuesto comando non richiede parametri aggiuntivi."
                                stop
                            else:
                                send "{@prefix_hide} &cComando sconosciuto."
                                stop
                    else:
                        if player has permission "axr.admin.world.add":
                            send "{@prefix} &cQuel mondo non esiste!"
                            send "{@prefix} &aMondi disponibili: &b%{all_worlds::*}%"
                            play sound "{@error_sound}" with volume 1 and pitch 0 at player for player
                            stop
                        else:
                            send "{@prefix_hide} &cComando sconosciuto."
                            stop
                else if arg-2 is "remove":
                    if arg-3 is not set:
                        if arg-4 is not set:
                            if player has permission "axr.admin.world.remove":
                                send "{@prefix} &cComando incompleto."
                                send "{@prefix} &cUtilizzo: &6/axr world remove <&emondo&6>"
                                stop
                            else:
                                send "{@prefix_hide} &cComando sconosciuto."
                                stop
                    else if arg-3 is "smp", "smp_nether" or "smp_the_end":
                        if arg-4 is not set:
                            if player has permission "axr.admin.world.remove":
                                set {removed_world} to arg-3 parsed as world
                                if {axr_worlds::*} is "&7Nessun mondo abilitato.":
                                    send "{@prefix} &cNon ci sono mondi che puoi rimuovere!"
                                    play sound "{@error_sound}" with volume 1 and pitch 0 at player for player
                                    stop
                                else if {all_worlds::*} is not "&7Nessun mondo disponibile.":
                                    send "{@prefix} &aHai rimosso &b%{removed_world}% &adalla lista dei mondi abilitati!"
                                    play sound "{@confirm_sound}" with volume 1 and pitch 2 at player for player
                                    remove {removed_world} from {axr_worlds::*}
                                    if {axr_worlds::*} is not set:
                                        set {axr_worlds::*} to "&7Nessun mondo abilitato."
                                    add {removed_world} to {all_worlds::*}
                                    stop
                            else:
                                send "{@prefix_hide} &cComando sconosciuto."
                                stop
                        else:
                            if player has permission "axr.admin.world.remove":
                                send "{@prefix} &cQuesto comando non richiede parametri aggiuntivi."
                                stop
                            else:
                                send "{@prefix_hide} &cComando sconosciuto."
                                stop
                    else:
                        if player has permission "axr.admin.world.remove":
                            send "{@prefix} &cQuel mondo non esiste!"
                            send "{@prefix} &4Puoi rimuovere: &6%{axr_worlds::*}%"
                            play sound "{@error_sound}" with volume 1 and pitch 0 at player for player
                            stop
                        else:
                            send "{@prefix_hide} &cComando sconosciuto."
                            stop
                else if arg-2 is "gui":
                    if arg-3 is not set:
                        if arg-4 is not set:
                            if player has permission "axr.admin.world.gui":
                                openAXRworlds(player)
                            else:
                                send "{@prefix_hide} &cComando sconosciuto."
                                stop
                        else:
                            if player has permission "axr.admin.world.gui":
                                send "{@prefix} &cQuesto comando non richiede parametri aggiuntivi."
                                stop
                            else:
                                send "{@prefix_hide} &cComando sconosciuto."
                                stop
                else if arg-2 isn't "add", "remove" or "gui":
                    if arg-3 is not set:
                        if arg-4 is not set:
                            if player has permission "axr.admin.world":
                                send "{@prefix} &cComando sconosciuto."
                                send "{@prefix} &cUtilizzo: &6/axr world <&eadd &6| &eremove &6| &egui&6>"
                                stop
                            else:
                                send "{@prefix_hide} &cComando sconosciuto."
                                stop
                    else:
                        if arg-4 is not set:
                            if player has permission "axr.admin.world":
                                send "{@prefix} &cComando sconosciuto."
                                send "{@prefix} &cUtilizzo: &6/axr world <&eadd &6| &eremove &6| &egui&6>"
                                stop
                            else:
                                send "{@prefix_hide} &cComando sconosciuto."
                                stop
                        else:
                            if player has permission "axr.admin.world":
                                send "{@prefix} &cComando sconosciuto."
                                send "{@prefix} &cUtilizzo: &6/axr world <&eadd &6| &eremove &6| &egui&6>"
                                stop
                            else:
                                send "{@prefix_hide} &cComando sconosciuto."
                                stop

            else if arg-1 is "ore":
                if arg-2 is not set:
                    if arg-3 is not set:
                        if arg-4 is not set:
                            if player has permission "axr.admin.ore":
                                send "{@prefix} &cComando incompleto."
                                send "{@prefix} &cUtilizzo: &6/axr ore <&eview &6| &ecounter &6| &eclear &6| &ealerts&6> [&egiocatore &6| &eall&6]"
                                stop
                            else:
                                send "{@prefix_hide} &cComando sconosciuto."
                                stop
                else if arg-2 is "view" or "check":
                    if arg-3 is not set:
                        if arg-4 is not set:
                            if player has permission "axr.admin.ore.viewer":
                                send "{@prefix} &cComando incompleto."
                                send "{@prefix} &cUtilizzo: &6/axr ore view for <&egiocatore&6>"
                                play sound "{@error_sound}" with volume 1 and pitch 0 at player for player
                                stop
                            else:
                                send "{@prefix_hide} &cComando sconosciuto."
                                stop
                    else if arg-3 is "for":
                        if arg-4 is not set:
                            if player has permission "axr.admin.ore.viewer":
                                send "{@prefix} &cSpecifica il nome di un giocatore!"
                                play sound "{@error_sound}" with volume 1 and pitch 0 at player for player
                                stop
                            else:
                                send "{@prefix_hide} &cComando sconosciuto."
                                stop
                        else if arg-4 is online:
                            if {axr::counter} is true:
                                send "{@prefix} &7Sto controllando i minerali scavati di &c%arg-4%&7..."
                                play sound "{@check_sound}" with volume 1 at player for player
                                wait 1 second
                                delete {checkplayer::latest}
                                set {checkplayer::latest} to arg-4
                                set {arg4} to arg-4
