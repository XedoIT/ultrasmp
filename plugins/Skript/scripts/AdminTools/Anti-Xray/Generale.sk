                                    send "{@prefix} &cNon ho trovato nessuna posizione per &6%arg-4%&c."
                                    play sound "{@error_sound}" with volume 1 and pitch 0 at player for player
                                    stop
                                else:
                                    send "{@prefix_hide} &cComando sconosciuto."
                                    stop
                            else:
                                if player has permission "axr.admin.offline.teleport":
                                    send "{@prefix} &7Ti sto portando all'ultima posizione conosciuta di &c%arg-4%&7..."
                                    wait 1 second
                                    teleport player to {axr_offline_location::%arg-4%}
                                    stop
                                else:
                                    send "{@prefix_hide} &cComando sconosciuto."
                                    stop
                    else if arg-3 is not "to":
                        if player has permission "axr.admin.offline.teleport":
                            send "{@prefix} &cComando sconosciuto."
                            send "{@prefix} &cUtilizzo: &6/axr offline teleport to <&egiocatore&6>"
                            play sound "{@error_sound}" with volume 1 and pitch 0 at player for player
                            stop
                        else:
                            send "{@prefix_hide} &cComando sconosciuto."
                            stop
                            
                else if arg-2 is not "toggle", "clear", "list", "view", "check" or "teleport":
                    if player has permission "axr.admin.offline":
                        send "{@prefix} &cComando sconosciuto."
                        send "{@prefix} &cUtilizzo: &6/axr offline toggle" 
                        send "{@prefix} &cUtilizzo: &6/axr offline clear all"
                        send "{@prefix} &cUtilizzo: &6/axr offline clear for <&egiocatore&6>"
                        send "{@prefix} &cUtilizzo: &6/axr offline view for <&egiocatore&6>" 
                        send "{@prefix} &cUtilizzo: &6/axr offline list" 
                        send "{@prefix} &cUtilizzo: &6/axr offline teleport to <&egiocatore&6>"
                        play sound "{@error_sound}" with volume 1 and pitch 0 at player for player
                        stop
                    else:
                        send "{@prefix_hide} &cComando sconosciuto."
                        stop
            else if arg-1 is "help":
                if arg-2 is not set:
                    if arg-3 is not set:
                        if arg-4 is not set:
                            if player has permission "axr.admin.help":
                                send "&cLISTA COMANDI PER {@prefix}"
                                send " &f- <suggest command:/axr>&6/axr<reset> &f- &7Apre la GUI principale."
                                send " &f- <suggest command:/axr item>&6/axr item<reset> &f- &7Ti dà un oggetto con cui aprire rapidamente la GUI dell'Anti-Xray."
                                send " &f- &6/axr world <<tooltip:&7Aggiungi un mondo all'Anti-Xray, così che possa essere controllato.><suggest command:/axr world add>&eadd<reset> &6| <tooltip:&7Rimuovi un mondo dal controllo dell'Anti-Xray.               &4&lATTENZIONE! &cIl mondo rimosso non sarà più controllato dall'Anti-Xray, pertanto, non verranno inviate notifiche degli avvisi dei minerali. Il contatore dei minerali continuerà a funzionare.><suggest command:/axr world remove>&eremove<reset> &6| <tooltip:&7Apre la GUI dei mondi dell'Anti-Xray.><suggest command:/axr world gui>&egui<reset>&6> &f- &7Gestisci i mondi dell'Anti-Xray."
                                send " &f- &6/axr ore <<tooltip:&7Visualizza i minerali scavati di un giocatore.><suggest command:/axr ore view for>&eview for<reset> &6| <tooltip:&7Gestisci le impostazioni del contatore dei minerali.><suggest command:/axr ore counter>&ecounter<reset> &6| <tooltip:&7Azzera i minerali scavati di un giocatore.><suggest command:/axr ore clear all>&eclear all<reset> &6| <tooltip:&7Attiva o disattiva gli avvisi dei minerali scavati.><suggest command:/axr ore alerts all>&ealerts&6><reset> &6[<tooltip:&7Per i comandi: &e/axr ore view for &7e &e/axr ore clear all>&egiocatore<reset> &6| <tooltip:&7Per il comando: &e/axr ore alerts>&eall<reset>&6] &f- &7Gestisci le impostazioni dei minerali."
                                send " &f- &6/axr settings <<tooltip:&7Modifica le impostazioni degli avvisi.><suggest command:/axr settings alerts>&ealerts<reset> &6| <tooltip:&7Modifica ogni quanto tempo i minerali possono essere scavati in modo sicuro.><suggest command:/axr settings timer>&etimer<reset> &6| <tooltip:&7Attiva o disattiva il contatore dei minerali.><suggest command:/axr settings counter>&ecounter<reset>&6> &f- &7Gestisci le impostazioni degli avvisi, del timer e del contatore dei minerali."
                                send " &f- <suggest command:/axr help>&6/axr help<reset> &f- &7Mostra questa lista."
                                send " &f- <suggest command:/axr reload>&6/axr reload<reset> &f- &7Ricarica l'Anti-Xray."
                                send ""
                                send "&e[&c&lN.B.&e] &aPuoi gestire tutte le impostazioni anche tramite le GUI!"
                                send "&a[&b&lTIP&a] &9Passa con il mouse sui parametri dei comandi per leggerne le descrizioni."
                                send "&a[&b&lTIP&a] &9Clicca sui comandi e sui parametri per riscriverli rapidamente in chat."
                                stop
                            else:
                                send "{@prefix_hide} &cComando sconosciuto."
                                stop
                else:
                    if player has permission "axr.admin.help":
                        send "{@prefix} &cQuesto comando non richiede parametri aggiuntivi."
                        stop
                    else:
                        send "{@prefix_hide} &cComando sconosciuto."
                        stop
            
            else if arg-1 is "reload":
                if arg-2 is not set:
                    if arg-3 is not set:
                        if arg-4 is not set:
                            if player has permission "axr.admin.reload":
                                send "{@prefix} &3Ricarico tutti i config..."
                                wait 1 second
                                send "{@prefix} &b- Ricarico il file generale..."
                                wait 1 second
                                send "{@prefix} &b- Ricarico le GUI:"
                                wait 1 second
                                send "{@prefix}  &9- Avvisi Offline..."
                                wait 3 ticks
                                send "{@prefix}  &9- Contatore Minerali..."
                                wait 3 ticks
                                send "{@prefix}  &9- Impostazioni..."
                                wait 3 ticks
                                send "{@prefix}  &9- Mondi..."
                                wait 3 ticks
                                send "{@prefix}  &9- Menu principale..."
                                wait 10 tick
                                execute console command "/skript reload {@axr_directory}"
                                send "{@prefix} &aConfig ricaricati! &7Puoi controllare se ci sono degli errori dalla console."
                                stop
                            else:
                                send "{@prefix_hide} &cComando sconosciuto."
                                stop
                else:
                    if player has permission "axr.admin.reload":
                        send "{@prefix} &cQuesto comando non richiede parametri aggiuntivi."
                        stop
                    else:
                        send "{@prefix_hide} &cComando sconosciuto."
                        stop