# INIZIO SCRIPT (ATTENZIONE!! Modifica il codice solo se sai ciò che fai!)
options:
 # GENERALI
    prefix: &6[&e&lAnti-Xray&6]
    prefix_hide: &8[&4&lINFO&8]
    enabled: &aattivato
    disabled: &cdisattivato
    version: 1.0
    axr_directory: \AdminTools\Anti-Xray\
 # SUONI
    confirm_sound: block.note_block.pling
    error_sound: entity.enderman.teleport
    check_sound: entity.player.levelup
 # NOTIFICA MINERALI
  # FERRO
    iron_alert_msg: &7ha scavato &f%{counter::%arg-player%::iron}% blocchi di minerale di ferro &7in &e%{timer}% &7secondi!
    deepslate_iron_alert_msg: &7ha scavato &f%{counter::%arg-player%::deepslate_iron}% blocchi di minerale di ferro in ardesia profonda &7in &e%{timer}% &7secondi!
  # RAME
    copper_alert_msg: &7ha scavato &d%{counter::%arg-player%::copper}% blocchi di minerale di rame &7in &e%{timer}% &7secondi!
    deepslate_copper_alert_msg: &7ha scavato &d%{counter::%arg-player%::deepslate_copper}% blocchi di minerale di rame in ardesia profonda &7in &e%{timer}% &7secondi!
  # ORO
    gold_alert_msg: &7ha scavato &6%{counter::%arg-player%::gold}% blocchi di minerale d'oro &7in &e%{timer}% &7secondi!
    deepslate_gold_alert_msg: &7ha scavato &6%{counter::%arg-player%::deepslate_gold}% blocchi di minerale d'oro in ardesia profonda &7in &e%{timer}% &7secondi!
  # REDSTONE
    redstone_alert_msg: &7ha scavato &4%{counter::%arg-player%::redstone}% blocchi di minerale di redstone &7in &e%{timer}% &7secondi!
    deepslate_redstone_alert_msg: &7ha scavato &4%{counter::%arg-player%::deepslate_redstone}% blocchi di minerale di redstone in ardesia profonda &7in &e%{timer}% &7secondi!
  # LAPISLAZZULI
    lapis_alert_msg: &7ha scavato &9%{counter::%arg-player%::lapis}% blocchi di minerale di lapislazzuli &7in &e%{timer}% &7secondi!
    deepslate_lapis_alert_msg: &7ha scavato &9%{counter::%arg-player%::deepslate_lapis}% blocchi di minerale di lapislazzuli in ardesia profonda &7in &e%{timer}% &7secondi!
  # SMERALDO
    emerald_alert_msg: &7ha scavato &a%{counter::%arg-player%::emerald}% blocchi di minerale di smeraldo &7in &e%{timer}% &7secondi!
    deepslate_emerald_alert_msg: &7ha scavato &a%{counter::%arg-player%::deepslate_emerald}% blocchi di minerale di smeraldo in ardesia profonda &7in &e%{timer}% &7secondi!
  # DIAMANTE
    diamond_alert_msg: &7ha scavato &b%{counter::%arg-player%::diamond}% blocchi di minerale di diamante &7in &e%{timer}% &7secondi!
    deepslate_diamond_alert_msg: &7ha scavato &b%{counter::%arg-player%::deepslate_diamond}% blocchi di minerale di diamante in ardesia profonda &7in &e%{timer}% &7secondi!
  # QUARZO DEL NETHER
    nether_quartz_alert_msg: &7ha scavato &f%{counter::%arg-player%::nether_quartz}% blocchi di minerale di quarzo del Nether &7in &e%{timer}% &7secondi!
  # ORO DEL NETHER
    nether_gold_alert_msg: &7ha scavato &6%{counter::%arg-player%::nether_gold}% blocchi di minerale d'oro del Nether &7in &e%{timer}% &7secondi!
  # DETRITI ANTICHI
    ancient_debris_alert_msg: &7ha scavato &f%{counter::%arg-player%::ancient_debris}% blocchi di detriti antichi &7in &e%{timer}% &7secondi!
  # MESSAGGIO NOTIFICA MINERALI
    hover_alert_msg: &7Clicca per teletrasportarti da &c%arg-player%
  # COMANDI MESSAGGIO NOTIFICA MINERALI
    alert_command1: /teleport %arg-player%
    #alert_command2: gmsp %player%
    #alert_command3: v %player%
 # RILEVAMENTO AUTOMATICO MINERALI
    auto_alert_msg: &e&l%arg-player% &c&lpotrebbe star usando l'Xray!
  # COMANDI RILEVAMENTO AUTOMATICO MINERALI (KICK): questi comandi saranno eseguiti ogni qualvolta il sistema rileverà che il giocatore stia usando l'Xray.
    auto_male_command1: kick %arg-player% due to "&c--- &8[&6&lS&e&lM&6&lP &f| &4&lAnti-Xray&8] &c---%nl%%nl%&cSei stato espulso dal server perché il sistema sospetta che tu stia usando l'Xray.%nl%&7Disattivala prima di rientrare nel server!"
    auto_male_command2: nothing
    auto_male_command3: nothing
    auto_male_command4: nothing
    auto_female_command1: kick %arg-player% due to "&c--- &8[&6&lS&e&lM&6&lP &f| &4&lAnti-Xray&8] &c---%nl%%nl%&cSei stata espulsa dal server perché il sistema sospetta che tu stia usando l'Xray.%nl%&7Disattivala prima di rientrare nel server!"
    auto_female_command2: nothing
    auto_female_command3: nothing
    auto_female_command4: nothing
  # NUMERO MASSIMO DI ESPULSIONI AUTOMATICHE: superato il valore sottostante si attiveranno i comandi a seguire.
    autokicktime: 3
  # COMANDI RILEVAMENTO AUTOMATICO MINERALI: questi comandi saranno eseguiti una volta superato il valore soprastante.
    max_warns_message: &6%arg-player% &cha raggiunto il limite massimo di avvertimenti!
    auto_next_command1: nothing
    auto_next_command2: nothing
    auto_next_command3: nothing
    auto_next_command4: nothing

on load:
 # CONFIGURAZIONE TIMER
    if {timer} is not set:
        set {timer} to 30
 # MINERALI SCAVATI
  # FERRO
    else if {iron_ore_mined_in_x_sec} is not set:
        set {iron_ore_mined_in_x_sec} to 20
    else if {deepslate_iron_ore_mined_in_x_sec} is not set:
        set {deepslate_iron_ore_mined_in_x_sec} to 20
  # RAME
    else if {copper_ore_mined_in_x_sec} is not set:
        set {copper_ore_mined_in_x_sec} to 15
    else if {deepslate_copper_ore_mined_in_x_sec} is not set:
        set {deepslate_copper_ore_mined_in_x_sec} to 15
  # ORO
    else if {gold_ore_mined_in_x_sec} is not set:
        set {gold_ore_mined_in_x_sec} to 15
    else if {deepslate_gold_ore_mined_in_x_sec} is not set:
        set {deepslate_gold_ore_mined_in_x_sec} to 15
  # REDSTONE
    else if {redstone_ore_mined_in_x_sec} is not set:
        set {redstone_ore_mined_in_x_sec} to 16
    else if {deepslate_redstone_ore_mined_in_x_sec} is not set:
        set {deepslate_redstone_ore_mined_in_x_sec} to 16
  # LAPISLAZZULI
    else if {lapis_ore_mined_in_x_sec} is not set:
        set {lapis_ore_mined_in_x_sec} to 11
    else if {deepslate_lapis_ore_mined_in_x_sec} is not set:
        set {deepslate_lapis_ore_mined_in_x_sec} to 11
  # SMERALDO
    else if {emerald_ore_mined_in_x_sec} is not set:
        set {emerald_ore_mined_in_x_sec} to 3
    else if {deepslate_emerald_ore_mined_in_x_sec} is not set:
        set {deepslate_emerald_ore_mined_in_x_sec} to 3
  # DIAMANTE
    else if {diamond_ore_mined_in_x_sec} is not set:
        set {diamond_ore_mined_in_x_sec} to 6
    else if {deepslate_diamond_ore_mined_in_x_sec} is not set:
        set {deepslate_diamond_ore_mined_in_x_sec} to 6
  # DETRITI ANTICHI
    else if {ancient_debris_mined_in_x_sec} is not set:
        set {ancient_debris_mined_in_x_sec} to 2
  # MINERALE DI QUARZO DEL NETHER
    else if {nether_quartz_ore_mined_in_x_sec} is not set:
        set {nether_quartz_ore_mined_in_x_sec} to 12
  # MINERALE D'ORO DEL NETHER
    else if {nether_gold_ore_mined_in_x_sec} is not set:
        set {nether_gold_ore_mined_in_x_sec} to 16
 # RICONOSCIMENTO AUTOMATICO MINERALI SCAVATI
  # FERRO (RICONOSCIMENTO AUTOMATICO)
    else if {detect_iron_ore_mined_in_x_sec} is not set:
        set {detect_iron_ore_mined_in_x_sec} to 30
    else if {detect_deepslate_iron_ore_mined_in_x_sec} is not set:
        set {detect_deepslate_iron_ore_mined_in_x_sec} to 30
  # RAME (RICONOSCIMENTO AUTOMATICO)
    else if {detect_copper_ore_mined_in_x_sec} is not set:
        set {detect_copper_ore_mined_in_x_sec} to 30
    else if {detect_deepslate_copper_ore_mined_in_x_sec} is not set:
        set {detect_deepslate_copper_ore_mined_in_x_sec} to 30
  # ORO (RICONOSCIMENTO AUTOMATICO)
    else if {detect_gold_ore_mined_in_x_sec} is not set:
        set {detect_gold_ore_mined_in_x_sec} to 25
    else if {detect_deepslate_gold_ore_mined_in_x_sec} is not set:
        set {detect_deepslate_gold_ore_mined_in_x_sec} to 25
  # REDSTONE (RICONOSCIMENTO AUTOMATICO)
    else if {detect_redstone_ore_mined_in_x_sec} is not set:
        set {detect_redstone_ore_mined_in_x_sec} to 16
    else if {detect_deepslate_redstone_ore_mined_in_x_sec} is not set:
        set {detect_deepslate_redstone_ore_mined_in_x_sec} to 16
  # LAPISLAZZULI (RICONOSCIMENTO AUTOMATICO)
    else if {detect_lapis_ore_mined_in_x_sec} is not set:
        set {detect_lapis_ore_mined_in_x_sec} to 11
    else if {detect_deepslate_lapis_ore_mined_in_x_sec} is not set:
        set {detect_deepslate_lapis_ore_mined_in_x_sec} to 11
  # SMERALDO (RICONOSCIMENTO AUTOMATICO)
    else if {detect_emerald_ore_mined_in_x_sec} is not set:
        set {detect_emerald_ore_mined_in_x_sec} to 7
    else if {detect_deepslate_emerald_ore_mined_in_x_sec} is not set:
        set {detect_deepslate_emerald_ore_mined_in_x_sec} to 7
  # DIAMANTE (RICONOSCIMENTO AUTOMATICO)
    else if {detect_diamond_ore_mined_in_x_sec} is not set:
        set {detect_diamond_ore_mined_in_x_sec} to 13
    else if {detect_deepslate_diamond_ore_mined_in_x_sec} is not set:
        set {detect_deepslate_diamond_ore_mined_in_x_sec} to 13
  # DETRITI ANTICHI (RICONOSCIMENTO AUTOMATICO)
    else if {detect_ancient_debris_mined_in_x_sec} is not set:
        set {detect_ancient_debris_mined_in_x_sec} to 7
  # MINERALE DI QUARZO DEL NETHER (RICONOSCIMENTO AUTOMATICO)
    else if {detect_nether_quartz_ore_mined_in_x_sec} is not set:
        set {detect_nether_quartz_ore_mined_in_x_sec} to 16
  # MINERALE D'ORO DEL NETHER (RICONOSCIMENTO AUTOMATICO)
    else if {detect_nether_gold_ore_mined_in_x_sec} is not set:
        set {detect_nether_gold_ore_mined_in_x_sec} to 21
 # CONFIGURAZIONE STATO VARIABILI SISTEMA
    else if {counter::status}, {autokick::status}, {orealerts::status}, {obf::status} or {consolenotify::status} is not set:
        set {counter::status}, {autokick::status}, {orealerts::status}, {obf::status} or {consolenotify::status} to "&cDISATTIVATO"
 # CONFIGURAZIONE STATO VARIABILI MINERALI
    else if {alert_iron::status}, {alert_deepslate_iron::status}, {alert_copper::status}, {alert_deepslate_copper::status}, {alert_gold::status}, {alert_deepslate_gold::status}, {alert_redstone::status}, {alert_deepslate_redstone::status}, {alert_lapis::status}, {alert_deepslate_lapis::status}, {alert_emerald::status}, {alert_deepslate::status}, {alert_diamond::status}, {alert_deepslate_diamond::status}, {alert_ancient_debris::status}, {autoalert_iron::status}, {autoalert_deepslate_iron::status}, {autoalert_copper::status}, {autoalert_deepslate_copper::status}, {autoalert_gold::status}, {autoalert_deepslate_gold::status}, {autoalert_redstone::status}, {autoalert_deepslate_redstone::status}, {autoalert_lapis::status}, {autoalert_deepslate_lapis::status}, {autoalert_emerald::status}, {autoalert_deepslate_emerald::status}, {autoalert_diamond::status}, {autoalert_deepslate_diamond::status}, {autoalert_ancient_debris::status}, {autoalert_nether_quartz_ore::status} or {autoalert_nether_gold_ore::status} is not set:
        set {alert_iron::status}, {alert_deepslate_iron::status}, {alert_copper::status}, {alert_deepslate_copper::status}, {alert_gold::status}, {alert_deepslate_gold::status}, {alert_redstone::status}, {alert_deepslate_redstone::status}, {alert_lapis::status}, {alert_deepslate_lapis::status}, {alert_emerald::status}, {alert_deepslate_emerald::status}, {alert_diamond::status}, {alert_deepslate_diamond::status}, {alert_ancient_debris::status}, {autoalert_iron::status}, {autoalert_deepslate_iron::status}, {autoalert_copper::status}, {autoalert_deepslate_copper::status}, {autoalert_gold::status}, {autoalert_deepslate_gold::status}, {autoalert_redstone::status}, {autoalert_deepslate_redstone::status}, {autoalert_lapis::status}, {autoalert_deepslate_lapis::status}, {autoalert_emerald::status}, {autoalert_deepslate_emerald::status}, {autoalert_diamond::status}, {autoalert_deepslate_diamond::status}, {autoalert_ancient_debris::status}, {autoalert_nether_quartz_ore::status} or {autoalert_nether_gold_ore::status}, {offline_orealerts::status} to "&aattivato"
 # CONFIGURAZIONE VARIABILI MINERALI SCAVATI DAL GIOCATORE
    loop all players:
        if {axr::%loop-player%::total_ores}, {axr::%loop-player%::iron_ore}, {axr::%loop-player%::deepslate_iron_ore}, {axr::%loop-player%::copper_ore}, {axr::%loop-player%::deepslate_copper_ore}, {axr::%loop-player%::gold_ore}, {axr::%loop-player%::deepslate_gold_ore}, {axr::%loop-player%::lapis_ore}, {axr::%loop-player%::deepslate_lapis_ore}, {axr::%loop-player%::redstone_ore}, {axr::%loop-player%::deepslate_redstone_ore}, {axr::%loop-player%::emerald_ore}, {axr::%loop-player%::deepslate_emerald_ore}, {axr::%loop-player%::diamond_ore}, {axr::%loop-player%::deepslate_diamond_ore}, {axr::%loop-player%::nether_quartz_ore}, {axr::%loop-player%::nether_gold_ore} or {axr::%loop-player%::ancient_debris} is not set:
            set {axr::%loop-player%::total_ores}, {axr::%loop-player%::iron_ore}, {axr::%loop-player%::deepslate_iron_ore}, {axr::%loop-player%::copper_ore}, {axr::%loop-player%::deepslate_copper_ore}, {axr::%loop-player%::gold_ore}, {axr::%loop-player%::deepslate_gold_ore}, {axr::%loop-player%::lapis_ore}, {axr::%loop-player%::deepslate_lapis_ore}, {axr::%loop-player%::redstone_ore}, {axr::%loop-player%::deepslate_redstone_ore}, {axr::%loop-player%::emerald_ore}, {axr::%loop-player%::deepslate_emerald_ore}, {axr::%loop-player%::diamond_ore}, {axr::%loop-player%::deepslate_diamond_ore}, {axr::%loop-player%::nether_quartz_ore}, {axr::%loop-player%::nether_gold_ore} or {axr::%loop-player%::ancient_debris} to 0
        else if {axr::%loop-player%::offline_total_ores}, {axr::%loop-player%::offline_iron_ore}, {axr::%loop-player%::offline_deepslate_iron_ore}, {axr::%loop-player%::offline_copper_ore}, {axr::%loop-player%::offline_deepslate_copper_ore}, {axr::%loop-player%::offline_gold_ore}, {axr::%loop-player%::offline_deepslate_gold_ore}, {axr::%loop-player%::offline_lapis_ore}, {axr::%loop-player%::offline_deepslate_lapis_ore}, {axr::%loop-player%::offline_redstone_ore}, {axr::%loop-player%::offline_deepslate_redstone_ore}, {axr::%loop-player%::offline_emerald_ore}, {axr::%loop-player%::offline_deepslate_emerald_ore}, {axr::%loop-player%::offline_diamond_ore}, {axr::%loop-player%::offline_deepslate_diamond_ore}, {axr::%loop-player%::offline_nether_quartz_ore}, {axr::%loop-player%::offline_nether_gold_ore} or {axr::%loop-player%::offline_ancient_debris} is not set:
            set {axr::%loop-player%::offline_total_ores}, {axr::%loop-player%::offline_iron_ore}, {axr::%loop-player%::offline_deepslate_iron_ore}, {axr::%loop-player%::offline_copper_ore}, {axr::%loop-player%::offline_deepslate_copper_ore}, {axr::%loop-player%::offline_gold_ore}, {axr::%loop-player%::offline_deepslate_gold_ore}, {axr::%loop-player%::offline_lapis_ore}, {axr::%loop-player%::offline_deepslate_lapis_ore}, {axr::%loop-player%::offline_redstone_ore}, {axr::%loop-player%::offline_deepslate_redstone_ore}, {axr::%loop-player%::offline_emerald_ore}, {axr::%loop-player%::offline_deepslate_emerald_ore}, {axr::%loop-player%::offline_diamond_ore}, {axr::%loop-player%::offline_deepslate_diamond_ore}, {axr::%loop-player%::offline_nether_quartz_ore}, {axr::%loop-player%::offline_nether_gold_ore} or {axr::%loop-player%::offline_ancient_debris} to 0
 # CONFIGURAZIONE MONDI
    if {axr_worlds::*} is not set:
        set {axr_worlds::*} to "&7Nessun mondo abilitato."
    if {all_worlds::*} is not set:
        loop all worlds:
            set {all_worlds::*} to all worlds
   # !! SOLO DEBUG !!
    #delete {axr_worlds::*}
    #delete {all_worlds::*}
    send "{@prefix} &9Sistema caricato e pronto! Versione di Minecraft: %minecraft version%" to console
on join:
