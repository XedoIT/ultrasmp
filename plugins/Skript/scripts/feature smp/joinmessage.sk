command /chatm [<text>] [<text>]:
	description: Cambia i tuoi messaggi di ingresso e di uscita del server.
	executable by: player, console
	trigger:
		if sender is console:
			if arg-1 is not set:
				send "&cQuesto comando può essere eseguito solo dai giocatori!"
				stop trigger
			else:
				send "&cQuesto comando può essere eseguito solo dai giocatori!"
				stop trigger
		else:
			if arg-1 is not set:
				execute player command "/chatmessage"
				stop trigger
				
			else if arg-1 is "none":
				if {chat::%player%::none} is true:
					play sound "entity.enderman.teleport" with volume 5 and pitch 0 at player for the player
					send "&cHai già selezionato questo pacchetto di messaggi!"
					set {chat::%player%::status} to "&aGià selezionato"
					stop trigger
				else:
					set {chat::%player%::none} to true
					set {chat::%player%::smp} to false
					set {chat::%player%::vanilla} to false
